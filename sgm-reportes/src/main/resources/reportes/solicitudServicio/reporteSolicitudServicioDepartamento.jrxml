	<parameter name="LOGO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT d.nombre,s.fecha_creacion,tr.id AS tramite,UPPER (it.nombre) as descripcion, s.descripcion_inconveniente as motivo, tr.estado,sd.accion,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_flow.sv_solicitud_servicios s
INNER JOIN sgm_flow.vu_items it ON(s.tipo_servicio=it.id)
INNER JOIN sgm_flow.sv_solicitud_departamento sd ON (s.id=sd.solicitud)
INNER JOIN sgm_app.ge_departamento d ON(sd.departamento=d.id)
INNER JOIN sgm_flow.historico_tramites tr ON(s.tramite=tr.id_tramite)
WHERE 	s.fecha_creacion>=$P{INICIO} AND s.fecha_creacion<$P{FIN}
	AND tr.estado = 'Pendiente'
ORDER BY d.nombre,s.tramite ASC;]]>
	</queryString>
