	<parameter name="inicio" class="java.util.Date"/>
	<parameter name="fin" class="java.util.Date"/>
	<parameter name="estado" class="java.lang.String"/>
	<queryString>
		<![CDATA[select s.fecha_creacion, s.tramite,s.descripcion_inconveniente ,h.nombre_propietario,
case s.asignado when true then sgm_flow.departamentos_solicitud_servicios(s.id) else 'NO ASIGANDA' end as "departamento", h.estado
from
   sgm_flow.sv_solicitud_servicios s inner join sgm_flow.historico_tramites h on(s.tramite=h.id_tramite)
WHERE
   s.fecha_creacion>=$P{inicio} AND s.fecha_creacion<$P{fin} AND h.estado ilike $P{estado}
order by s.tramite ASC]]>
	</queryString>
