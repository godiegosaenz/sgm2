	<parameter name="parameter1" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     sss."id" AS id,
     sss."direccion" AS direccion,
     sss."descripcion_inconveniente" AS descripcion_inconveniente,
     vi."nombre" AS tipo_serv_nombre,
     sss."status" AS status,
     sss."asignados" AS asignados,
     sss."asignado" AS asignado,
     ht."id" AS tramite,
     ht."estado" AS estado,
     ht."id_proceso" AS id_proceso,
     sss."fecha_creacion" AS fecha_creacion,
     ht."nombre_propietario" AS nombre_propietario
FROM
     sgm_flow."vu_items" vi INNER JOIN sgm_flow."sv_solicitud_servicios" sss ON vi."id" = sss."tipo_servicio"
     INNER JOIN sgm_flow."historico_tramites" ht ON sss."tramite" = ht."id_tramite"
     INNER JOIN sgm_flow."sv_solicitud_departamento" sv_solicitud_departamento ON sss."id" = sv_solicitud_departamento."solicitud"
WHERE
     CASE $P{desde} WHEN  NOT NULL THEN sss.fecha_creacion >= $P{desde} END
     AND CASE $P{hasta} WHEN  NOT NULL THEN sss.fecha_creacion <= $P{hasta} END
     AND CASE $P{estado} WHEN  NOT NULL THEN ht.estado = $P{estado} END]]>
	</queryString>
