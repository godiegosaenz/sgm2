	<parameter name="LOGO" class="java.lang.String"/>
	<parameter name="INICIO" class="java.util.Date"/>
	<parameter name="FIN" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT s.fecha_creacion,s.tramite,
CASE p.es_persona WHEN TRUE THEN
	coalesce(p.nombres,'')||' '||coalesce(p.apellidos,'')
        ELSE p.razon_social END AS nombre_del_solicitante,
(SELECT t.telefono FROM sgm_app.ente_telefono t WHERE t.ente = s.ente_solicitante ORDER BY t.id DESC LIMIT 1)  as telefono,
(SELECT c.email FROM sgm_app.ente_correo c WHERE c.ente = s.ente_solicitante ORDER BY c.id DESC LIMIT 1) as correounico,

UPPER (it.nombre) as descripcion, s.descripcion_inconveniente as motivo,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa

FROM sgm_flow.sv_solicitud_servicios s
LEFT OUTER JOIN sgm_app.cat_ente p ON (s.ente_solicitante=p.id)
LEFT OUTER JOIN sgm_flow.historico_tramites tr ON (s.tramite=tr.id_tramite)
INNER JOIN sgm_flow.vu_items it ON(s.tipo_servicio=it.id)
WHERE 	s.fecha_creacion>=$P{INICIO} AND s.fecha_creacion<$P{FIN} AND (s.lugar_audiencia IS NULL OR s.lugar_audiencia=785) AND NOT s.asignado
ORDER BY it.nombre ASC,s.tramite ASC;]]>
	</queryString>
