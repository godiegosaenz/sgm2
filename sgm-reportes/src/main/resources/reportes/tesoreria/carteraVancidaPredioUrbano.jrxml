	<queryString>
		<![CDATA[SELECT L.ANIO , SUM(L.SALDO)AS TOTAL ,PA.DESCRIPCION AS PARROQUIA,
  (SELECT RAZON_SOCIAL FROM SGM_APPLICATION.EMPRESA)AS RAZON_SOCIAL
  FROM SGM_FINANCIERO.REN_LIQUIDACION L
  INNER JOIN SGM_APP.CAT_PREDIO P ON (L.PREDIO = P.ID)
  INNER JOIN SGM_APP.CAT_PARROQUIA PA ON (P.PARROQUIA = PA.ID)
  WHERE  L.TIPO_LIQUIDACION = 13 AND L.ESTADO_LIQUIDACION = 2 AND L.ANIO < $P{ANIO}
  GROUP BY L.ANIO, PA.DESCRIPCION
  ORDER BY ANIO DESC;]]>
	</queryString>
