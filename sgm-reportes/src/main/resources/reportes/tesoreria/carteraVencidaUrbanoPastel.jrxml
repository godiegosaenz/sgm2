	<queryString>
		<![CDATA[-- CARTERA VENCIDA PRDIOS-RURALES

SELECT SUM(L.TOTAL_PAGO) AS TOTAL, L.ANIO, PA.DESCRIPCION AS PARROQUIA,
(SELECT RAZON_SOCIAL FROM SGM_APPLICATION.EMPRESA) AS RAZON_SOCIAL

FROM SGM_FINANCIERO.REN_LIQUIDACION L
INNER JOIN SGM_APP.CAT_PREDIO P ON (L.PREDIO = P.ID)
INNER JOIN SGM_APP.CAT_PROVINCIA PR ON pr.cod_nac=p.provincia
INNER JOIN SGM_APP.CAT_CANTON cc ON (cc.cod_nac=p.canton AND cc.id_provincia=pr.id)
INNER JOIN SGM_APP.CAT_PARROQUIA PA ON (P.PARROQUIA = PA.codigo_parroquia AND PA.id_canton=cc.id)

WHERE L.TIPO_LIQUIDACION = 13 AND L.ESTADO_LIQUIDACION = 2
GROUP BY   L.ANIO,PA.DESCRIPCION
HAVING SUM(L.TOTAL_PAGO) > 0
ORDER BY L.ANIO , PA.DESCRIPCION DESC;]]>
	</queryString>
