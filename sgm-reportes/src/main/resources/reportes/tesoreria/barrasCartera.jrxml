<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="barrasCartera" language="groovy" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="802" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="acb563ef-9ccc-4685-a65e-9e468319df4b">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ANIO" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT SUM(L.TOTAL_PAGO) AS TOTAL, L.ANIO, PA.DESCRIPCION AS PARROQUIA
FROM SGM_FINANCIERO.REN_LIQUIDACION L
INNER JOIN SGM_APP.CAT_PREDIO P ON (L.PREDIO = P.ID)
INNER JOIN SGM_APP.CAT_PROVINCIA PR ON pr.cod_nac=p.provincia
INNER JOIN SGM_APP.CAT_CANTON cc ON (cc.cod_nac=p.canton AND cc.id_provincia=pr.id)
INNER JOIN SGM_APP.CAT_PARROQUIA PA ON (P.PARROQUIA = PA.codigo_parroquia AND PA.id_canton=cc.id)

WHERE L.TIPO_LIQUIDACION = 13 AND L.ESTADO_LIQUIDACION = 2
AND L.ANIO = $P{ANIO}
GROUP BY L.ANIO,PA.DESCRIPCION
ORDER BY L.ANIO , PA.DESCRIPCION DESC;]]>
	</queryString>
	<field name="total" class="java.math.BigDecimal"/>
	<field name="anio" class="java.lang.Integer"/>
	<field name="parroquia" class="java.lang.String"/>
