	<queryString>
		<![CDATA[SELECT
   CP.num_predio,CP.sector||'-'||CP.mz||'-'||CP.cdla||'-'||CP.mzdiv||'-'||CP.solar||'-'||CP.div1||'-'||CP.div2||'-'||CP.div3||'-'||CP.div4||'-'||CP.div5||'-'||CP.div6||'-'||CP.div7||'-'||CP.div8||'-'||CP.div9||'-'||CP.phv||'-'||CP.phh as cod_predial,
    CP.predialant,
   'Mz: '||COALESCE(CP.urb_mz, '')||' Sl:'||COALESCE(CP.urb_solarnew, '') as direccion,
   l.saldo,
   CASE WHEN l.saldo<l.total_pago THEN 'S' ELSE 'N' END AS estado,
   l.anio,
   P.VALOR,
   P.FECHA_PAGO,
   PD.TR_FECHA,
   B.DESCRIPCION,
  (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_financiero.REN_PAGO P
   INNER JOIN sgm_financiero.REN_LIQUIDACION L ON P.LIQUIDACION = L.ID
   INNER JOIN sgm_financiero.REN_PAGO_DETALLE PD ON PD.PAGO = P.ID
   INNER JOIN sgm_app.CAT_PREDIO CP ON CP.ID = L.PREDIO
   INNER JOIN sgm_financiero.REN_ENTIDAD_BANCARIA B ON B.ID = PD.TR_BANCO
WHERE
     P.ESTADO
 AND (CAST(PD.TR_FECHA AS DATE) BETWEEN $P{DESDE} AND $P{HASTA})
 AND PD.TIPO_PAGO = 5 AND PD.TR_BANCO = $P{BANCO};]]>
	</queryString>
