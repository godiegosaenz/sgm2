	<queryString>
		<![CDATA[SELECT p.reg_catastral, p.id_predial, p.nombre_predio, p.sitio, p.num_predio_rustico,
l.anio, l.saldo, pq.descripcion AS parroquia, er.codigo_catastral || ' - ' || er.parroquia AS codigo_catastral_nuevo,
(TRIM(COALESCE(e.apellidos, '') || ' ' || COALESCE(e.nombres, '')) || TRIM(COALESCE(e.razon_social, ''))) as propietario,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_app.cat_predio_rustico p
LEFT OUTER JOIN sgm_app.emisiones_rurales_excel er ON (er.predio_rustico=p.id)
LEFT OUTER JOIN sgm_app.cat_ente e ON (p.propietario=e.id)
LEFT OUTER JOIN (SELECT predio_rustico,rural_excel,anio, saldo FROM sgm_financiero.ren_liquidacion WHERE tipo_liquidacion=7 AND estado_liquidacion=2) AS l ON (l.predio_rustico=p.id OR l.rural_excel=er.id )
LEFT OUTER JOIN sgm_app.cat_parroquia pq ON pq.id = p.parroquia
WHERE $X{IN,p.id,PREDIOS} AND p.estado
ORDER BY p.id_predial,l.anio ASC;]]>
	</queryString>
