	<queryString>
		<![CDATA[SELECT
     CASE WHEN PROP.ES_PERSONA THEN COALESCE(PROP.NOMBRES)||' '||COALESCE(PROP.APELLIDOS)ELSE COALESCE(PROP.RAZON_SOCIAL)END AS REP_LEGAL,
     L.NUM_LIQUIDACION AS L_NUM_LIQUIDACION,
     L.TOTAL_PAGO AS L_TOTAL_PAGO,
     L.FECHA_INGRESO AS L_FECHA_INGRESO,
     LC.NOMBRE_LOCAL AS LC_NOMBRE_LOCAL,
     LC.NUM_LOCAL AS LC_NUM_LOCAL,
     LC.AREA AS LC_AREA,
     U.DESCRIPCION AS U_DESCRIPCION,
     PROP.CI_RUC AS PROP_CI_RUC,
     TL.NOMBRE_TITULO AS TL_NOMBRE_TITULO,
     TL.NOMBRE_TRANSACCION AS TL_NOMBRE_TRANSACCION,
     L.ANIO AS L_ANIO
FROM
     SGM_FINANCIERO.REN_TIPO_LIQUIDACION TL
     INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION L ON TL.ID = L.TIPO_LIQUIDACION
     INNER JOIN SGM_FINANCIERO.REN_LOCAL_COMERCIAL LC ON L.LOCAL_COMERCIAL = LC.ID
     LEFT OUTER JOIN SGM_FINANCIERO.REN_LOCAL_UBICACION U ON LC.UBICACION = U.ID
     INNER JOIN SGM_APP.CAT_ENTE PROP ON LC.PROPIETARIO = PROP.ID
WHERE
     L.ESTADO_LIQUIDACION = 2
 AND L.FECHA_INGRESO::DATE BETWEEN $P{DESDE} AND $P{HASTA}
ORDER BY
     U.DESCRIPCION ASC]]>
	</queryString>
