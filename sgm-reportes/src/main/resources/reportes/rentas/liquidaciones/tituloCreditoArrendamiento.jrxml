	<queryString>
		<![CDATA[select CASE WHEN (UPPER(f_convnl(liq.total_pago)) LIKE '%CON%') THEN

UPPER(f_convnl(liq.total_pago)) || ' CENTAVOS' ELSE UPPER(f_convnl(liq.total_pago)) || ' DÓLARES' END AS total_letras

, id_liquidacion , liq.fecha_ingreso::date, total_pago,

ente.ci_ruc,
local.id codigo_local,
coalesce(ente.apellidos, '') || ' ' || coalesce(ente.nombres, '') || ' ' || coalesce(ente.razon_social, '') as propietario,
local.nombre_local, local.area, local.num_local,

(select string_agg(UPPER(act.descripcion), ', ' order by act.id)
  from sgm_financiero.ren_actividad_por_local rapl
  INNER JOIN sgm_financiero.ren_actividad_comercial act ON act.id = rapl.actividad
  WHERE rapl.local_comercial = local.id) as actividades,

  sgm_app.get_mes_letra(liq.mes) || ' - ' || liq.anio as fecha_emision

from sgm_financiero.ren_liquidacion liq
inner join sgm_financiero.ren_local_comercial local on liq.local_comercial = local.id
inner join sgm_app.cat_ente ente on ente.id = liq.comprador
where liq.id = $P{ID}]]>
	</queryString>
