	<subDataset name="RUBROS" uuid="9b9997fc-0d56-4fc9-b485-904b53183c83">
		<parameter name="ID_LIQUIDACION" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="NUM_COMPROBANTE" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
   RL.DESCRIPCION  AS DESCRIPCION,

    RL.RUBRO_DEL_MUNICIPIO,
    CASE
     WHEN
        (SELECT PR.VALOR  FROM
	SGM_FINANCIERO.REN_PAGO_RUBRO PR  INNER JOIN SGM_FINANCIERO.REN_PAGO P ON P.ID = PR.PAGO
	WHERE P.LIQUIDACION = $P{ID_LIQUIDACION} AND P.NUM_COMPROBANTE = $P{NUM_COMPROBANTE}  AND PR.RUBRO = RL.ID) > 0 THEN
	(SELECT PR.VALOR  FROM
	SGM_FINANCIERO.REN_PAGO_RUBRO PR  INNER JOIN SGM_FINANCIERO.REN_PAGO P ON P.ID = PR.PAGO
	WHERE P.LIQUIDACION = $P{ID_LIQUIDACION} AND P.NUM_COMPROBANTE = $P{NUM_COMPROBANTE}  AND PR.RUBRO = RL.ID)
	ELSE 0.00 END AS VALOR_RECAUDADO
FROM
   SGM_FINANCIERO.REN_RUBROS_LIQUIDACION RL
   WHERE RL.TIPO_LIQUIDACION = 13 AND RL.CODIGO_RUBRO NOT IN (10, 11, 12, 13, 14, 15, 16)
ORDER BY RL.PRIORIDAD ASC;]]>
		</queryString>
		<field name="descripcion" class="java.lang.String"/>
		<field name="rubro_del_municipio" class="java.lang.Boolean"/>
		<field name="valor_recaudado" class="java.math.BigDecimal"/>
	</subDataset>
