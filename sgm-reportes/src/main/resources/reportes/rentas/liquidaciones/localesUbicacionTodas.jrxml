	<queryString>
		<![CDATA[SELECT
     CASE WHEN PROP.ES_PERSONA THEN COALESCE(PROP.NOMBRES)||' '||COALESCE(PROP.APELLIDOS)ELSE COALESCE(PROP.RAZON_SOCIAL)END AS REP_LEGAL,
     LC.NOMBRE_LOCAL AS LC_NOMBRE_LOCAL,
     LC.NUM_LOCAL AS LC_NUM_LOCAL,
     AC.DESCRIPCION AS AC_DESCRIPCION,
     U.DESCRIPCION AS U_DESCRIPCION,
     CC.NOMBRE AS CC_NOMBRE,
     PROP.CI_RUC AS PROP_CI_RUC,
     LC.ESTADO_LOCAL AS ESTADO,
     CP.DESCRIPCION AS PARROQUIA
FROM
     SGM_APP.CAT_ENTE PROP RIGHT OUTER JOIN SGM_FINANCIERO.REN_LOCAL_COMERCIAL LC ON PROP.ID = LC.PROPIETARIO
     LEFT OUTER JOIN SGM_FINANCIERO.REN_LOCAL_UBICACION U ON LC.UBICACION = U.ID
     LEFT OUTER JOIN SGM_FINANCIERO.REN_ACTIVIDAD_POR_LOCAL AXL ON LC.ID = AXL.LOCAL_COMERCIAL
     LEFT OUTER JOIN SGM_FINANCIERO.REN_ACTIVIDAD_COMERCIAL AC ON AXL.ACTIVIDAD = AC.ID
     LEFT OUTER JOIN SGM_APP.CAT_CIUDADELA CC ON U.CIUDADELA = CC.ID
     LEFT OUTER JOIN SGM_APP.CAT_PARROQUIA CP ON CP.ID = CC.COD_PARROQUIA
WHERE
     CASE WHEN $P{ESTADO_LOCAL} IS NULL THEN TRUE ELSE LC.ESTADO_LOCAL = $P{ESTADO_LOCAL} END
GROUP BY
     CP.DESCRIPCION, LC.ESTADO_LOCAL, PROP.ES_PERSONA, PROP.NOMBRES, PROP.APELLIDOS, PROP.RAZON_SOCIAL, LC.NOMBRE_LOCAL,
     LC.NUM_LOCAL, AC.DESCRIPCION, U.DESCRIPCION, CC.NOMBRE, PROP.CI_RUC
ORDER BY
     CP.DESCRIPCION ASC , U.DESCRIPCION ASC]]>
	</queryString>
