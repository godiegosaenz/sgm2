	<subDataset name="LIQUIDACION_LOCALES" uuid="7add75b2-d56d-42e5-9862-f023a4ed677a">
		<parameter name="ID" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    ANIO ,
      CASE
       WHEN MES = 1
   THEN 'ENERO'
       WHEN MES = 2
   THEN 'FEBRERO'
       WHEN MES = 3
   THEN 'MARZO'
       WHEN MES = 4
   THEN 'ABRIL'
       WHEN MES = 5
   THEN 'MAYO'
       WHEN MES = 6
   THEN 'JUNIO'
       WHEN MES = 7
   THEN 'JULIO'
       WHEN MES = 8
   THEN 'AGOSTO'
       WHEN MES = 9
   THEN 'SEPTIEMBRE'
       WHEN MES = 10
   THEN 'OCTUBRE'
       WHEN MES = 11
   THEN 'NOVIEMBRE'
       WHEN MES = 12
   THEN 'DICIEMBRE'
  END AS MES,
    TOTAL_PAGO ,
  CASE
       WHEN ESTADO_LIQUIDACION = 1
   THEN 'PAGADO'
       WHEN ESTADO_LIQUIDACION = 2
   THEN 'POR PAGAR'
  END AS ESTADO_LIQUIDACION
FROM SGM_FINANCIERO.REN_LIQUIDACION
WHERE LOCAL_COMERCIAL = $P{ID}
ORDER BY ESTADO_LIQUIDACION ASC  LIMIT 4]]>
		</queryString>
		<field name="anio" class="java.lang.Integer"/>
		<field name="mes" class="java.lang.String"/>
		<field name="total_pago" class="java.math.BigDecimal"/>
		<field name="estado_liquidacion" class="java.lang.String"/>
	</subDataset>
	<parameter name="LOGO" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ID" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     CASE WHEN CE.ES_PERSONA THEN COALESCE(CE.NOMBRES)||' '||COALESCE(CE.APELLIDOS)ELSE
	 COALESCE(CE.RAZON_SOCIAL)END AS REP_LEGAL,
     LC.NOMBRE_LOCAL AS LC_NOMBRE_LOCAL,
     LC.NUM_LOCAL    AS LC_NUM_LOCAL,
     LC.AREA         AS LC_AREA,
     CC.NOMBRE       AS CC_NOMBRE,
     CE.CI_RUC     AS PROP_CI_RUC,
     (SELECT STRING_AGG(UPPER(ACT.DESCRIPCION), ', ' ORDER BY ACT.ID)
      FROM SGM_FINANCIERO.REN_ACTIVIDAD_POR_LOCAL RAPL
      INNER JOIN SGM_FINANCIERO.REN_ACTIVIDAD_COMERCIAL ACT ON ACT.ID = RAPL.ACTIVIDAD
      WHERE RAPL.LOCAL_COMERCIAL = LC.ID) AS ACTIVIDADES
FROM
     SGM_APP.CAT_ENTE  CE
	 RIGHT OUTER JOIN SGM_FINANCIERO.REN_LOCAL_COMERCIAL      LC  ON CE.ID         = LC.PROPIETARIO
     LEFT OUTER JOIN  SGM_FINANCIERO.REN_LOCAL_UBICACION      U   ON LC.UBICACION  = U.ID
     LEFT OUTER JOIN  SGM_APP.CAT_CIUDADELA                   CC  ON U.CIUDADELA   = CC.ID
WHERE LC.ID = $P{ID}]]>
	</queryString>
