	<queryString>
		<![CDATA[SELECT
    EL.descripcion AS estado_liquidacion,
    l.num_comprobante AS NUM_COMPROBANTE,
    l.num_liquidacion AS NUM_LIQUIDACION,
    L.fecha_ingreso AS fecha_ingreso,
    L.OBSERVACION AS OBSERVACION,
    L.TOTAL_PAGO AS TOTAL,
    b.capital AS capital,
    b.anio_balance AS anio_balance,
    b.fecha_balance AS fecha_balance,
    LU.DESCRIPCION AS UBICACION,
    LC.NUM_LOCAL AS NUM_LOCAL,
    LC.NOMBRE_LOCAL AS NOMBRE_LOCAL,
    CASE WHEN E.ES_PERSONA THEN E.APELLIDOS || ' ' ||E.NOMBRES ELSE E.RAZON_SOCIAL END,
    E.CI_RUC AS RUC,
    CASE WHEN rz.ES_PERSONA THEN rz.APELLIDOS || ' ' ||rz.NOMBRES ELSE rz.RAZON_SOCIAL END AS razon_social_local,
    rz.CI_RUC AS RUC_razon_social
FROM
     sgm_financiero.ren_liquidacion L LEFT OUTER JOIN sgm_financiero.ren_local_comercial LC ON LC.id = L.local_comercial
     LEFT OUTER JOIN sgm_financiero.ren_tipo_liquidacion TL ON L.tipo_liquidacion = TL.id
     LEFT OUTER JOIN sgm_app.cat_ente E ON E.id = L.comprador
     LEFT OUTER JOIN sgm_financiero.ren_local_ubicacion LU ON LC.ubicacion = LU.id
     LEFT OUTER JOIN sgm_financiero.ren_balance_local_comercial b ON L.id = b.num_liquidacion
     LEFT OUTER JOIN sgm_app.cat_ente rz ON rz.id = LC.razon_social
     LEFt OUTER JOIN sgm_financiero.ren_estado_liquidacion El ON EL.ID =L.ESTADO_LIQUIDACION


WHERE
     L.ID = $P{LIQUIDACION}
 AND b.estado = true]]>
	</queryString>
