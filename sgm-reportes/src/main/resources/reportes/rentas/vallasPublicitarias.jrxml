	<queryString>
		<![CDATA[SELECT
    l.num_liquidacion AS NUM_LIQUIDACION,
    L.FECHA_INGRESO AS fecha_ingreso,
    L.OBSERVACION AS OBSERVACION,
    L.CODIGO_LOCAL AS CODIGO_LOCAL,
    L.AREA_TOTAL AS AREA_TOTAL,
    L.TOTAL_PAGO AS TOTAL_PAGO,
    LU.DESCRIPCION AS UBICACION,
    CC.cod_parroquia AS parroquia,
    LC.NUM_LOCAL AS NUM_LOCAL,
    CASE WHEN E.ES_PERSONA THEN
       E.APELLIDOS || ' ' ||E.NOMBRES
    ELSE
       E.RAZON_SOCIAL END,
    E.CI_RUC AS RUC,
    CASE WHEN rz.ES_PERSONA THEN
       rz.APELLIDOS || ' ' ||rz.NOMBRES
    ELSE
       rz.RAZON_SOCIAL END AS razon_social_local,
    rz.CI_RUC AS RUC_razon_social,
    L.ID,

(SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM
     sgm_financiero.ren_liquidacion L
     INNER JOIN sgm_financiero.ren_local_comercial LC ON LC.id = L.local_comercial
     INNER JOIN sgm_financiero.ren_tipo_liquidacion TL ON L.tipo_liquidacion = TL.id
     INNER JOIN sgm_app.cat_ente E ON E.id = L.comprador
     LEFT OUTER JOIN sgm_financiero.ren_actividad_por_local ACTL ON LC.id = ACTL.local_comercial
     LEFT OUTER JOIN sgm_financiero.ren_actividad_comercial ACC ON ACTL.actividad = ACC.id
     LEFT OUTER JOIN sgm_financiero.ren_local_ubicacion LU ON LC.ubicacion = LU.id
     LEFT OUTER JOIN sgm_app.cat_ente rz ON rz.id = LC.razon_social
     LEFT OUTER JOIN sgm_app.cat_ciudadela cc ON cc.id = LU.ciudadela
WHERE
     L.ID = $P{LIQUIDACION}
LIMIT 1]]>
	</queryString>
