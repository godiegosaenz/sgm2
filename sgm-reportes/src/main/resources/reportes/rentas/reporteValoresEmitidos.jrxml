	<queryString>
		<![CDATA[SELECT COUNT(RL.ID) AS TOTAL_PREDIO_PROCESADOS ,
(SELECT RAZON_SOCIAL FROM SGM_APPLICATION.EMPRESA where id=1)AS RAZON_SOCIAL,
(SELECT COUNT(CP.ID) FROM SGM_APP.CAT_PREDIO CP
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION RL ON CP.ID=RL.PREDIO
INNER JOIN SGM_FINANCIERO.FN_EXONERACION_LIQUIDACION EL ON RL.ID=EL.LIQUIDACION_POSTERIOR
WHERE RL.TIPO_LIQUIDACION IN (7,13) AND CP.ESTADO='A' AND RL.ANIO=$P{ANIO}) AS TOTAL_EXONERADO,
(SELECT COUNT(CP.ID) FROM SGM_APP.CAT_PREDIO CP
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION RL ON CP.ID=RL.PREDIO
INNER JOIN SGM_FINANCIERO.FN_EXONERACION_LIQUIDACION EL ON RL.ID=EL.LIQUIDACION_POSTERIOR
INNER JOIN SGM_FINANCIERO.FN_SOLICITUD_EXONERACION SE ON SE.ID=EL.EXONERACION
WHERE RL.TIPO_LIQUIDACION IN (7,13) AND CP.ESTADO='A'AND SE.EXONERACION_TIPO=17 AND rl.exonerado='t'  AND RL.ANIO=$P{ANIO} ) AS TOTAL_LEY_DEL_ANCIANO,
(SELECT COUNT(CP.ID) FROM SGM_APP.CAT_PREDIO CP
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION RL ON CP.ID=RL.PREDIO
INNER JOIN SGM_FINANCIERO.FN_EXONERACION_LIQUIDACION EL ON RL.ID=EL.LIQUIDACION_POSTERIOR
INNER JOIN SGM_FINANCIERO.FN_SOLICITUD_EXONERACION SE ON SE.ID=EL.EXONERACION
WHERE RL.TIPO_LIQUIDACION IN (7,13) AND CP.ESTADO='A'AND SE.EXONERACION_TIPO=34 AND CP.TENENCIA IN (1,2,4,5 )AND rl.exonerado='t'  AND RL.ANIO=$P{ANIO}) AS TOTAL_PUBLICO
FROM  SGM_APP.CAT_PREDIO CP
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION RL ON CP.ID=RL.PREDIO
WHERE RL.TIPO_LIQUIDACION IN (7,13) AND CP.ESTADO='A' AND RL.ANIO=$P{ANIO}]]>
	</queryString>
