	<subDataset name="VALORES" uuid="b5710d3f-f45e-41f4-ba8b-20969c76d507">
		<queryString language="SQL">
			<![CDATA[select bl.descripcion, sum(dl.valor) as total_pagar from sgm_financiero.ren_liquidacion rl
inner join sgm_app.cat_predio cp on cp.id=rl.predio
inner join sgm_financiero.ren_det_liquidacion dl on dl.liquidacion=rl.id
inner join sgm_financiero.ren_rubros_liquidacion bl on bl.id=dl.rubro
where cp.estado='A' and rl.tipo_liquidacion=13 and rl.estado_liquidacion=2 and bl.id<>8
group by bl.descripcion]]>
		</queryString>
		<field name="descripcion" class="java.lang.String"/>
		<field name="total_pagar" class="java.math.BigDecimal"/>
	</subDataset>
