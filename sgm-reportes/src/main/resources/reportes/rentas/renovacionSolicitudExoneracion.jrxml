	<queryString>
		<![CDATA[SELECT
    CASE WHEN se.exoneracion_tipo = 17 OR se.exoneracion_tipo = 18 THEN TRUE END AS L_ANCIANO,
    CASE WHEN se.exoneracion_tipo = 37 OR se.exoneracion_tipo = 44 THEN TRUE END AS L_DISCAPACIDAD,
    sa.anio,
    se.num_resolucion_sac,
    sol.ci_ruc AS cedula_solic,
    CASE WHEN sol.es_persona THEN COALESCE(sol.nombres,'') || ' ' || COALESCE(sol.apellidos, '') ELSE COALESCE(sol.razon_social,'') END AS NOMBRE_SOL,
    sol.direccion AS direccion_solic,
    sol.fecha_nacimiento as fecha_nacimiento_solic,
    (SELECT T.TELEFONO FROM sgm_app.ENTE_TELEFONO T WHERE T.ENTE = sol.id ORDER BY T.ID DESC LIMIT 1) AS telefono_sol,
    (SELECT C.EMAIL FROM sgm_app.ENTE_CORREO C WHERE C.ENTE = sol.id ORDER BY C.ID DESC LIMIT 1) AS correo_sol,
    (cp.sector|| '-' || cp.mz || '-' || cp.cdla || '-' || cp.mzdiv || '-' || COALESCE(cp.solar, 0) || '-' ||cp.div1 || '-' || cp.div2|| '-' ||  cp.div3  || '-' ||cp.div4 || '-' ||cp.div5|| '-' || cp.div6|| '-' ||  cp.div7  || '-' ||cp.div8 || '-' ||cp.div9 || '-' ||cp.phv || '-' ||cp.phh) AS cod_predio_actual,
    (cpr.reg_catastral || '-'|| cpr.id_predial || '-' || COALESCE(cpq.codigo_parroquia, '0')) AS codigo_predio_rustico,
    (SELECT la.total_pago FROM sgm_financiero.ren_liquidacion la WHERE la.estado_liquidacion = 4 AND la.anio = sa.anio AND la.predio = se.predio OR la.predio_rustico = se.predio_rustico) AS VALOR_ANT,
    l.total_pago AS VALOR_ACT,
    l.avaluo_municipal AS avaluo_municipal,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa

FROM
   sgm_financiero.fn_solicitud_exoneracion se
   INNER JOIN sgm_financiero.fn_solicitud_exoneracion_automatica sa ON se.id = sa.solicitud_exoneracion
   INNER JOIN sgm_app.cat_ente sol ON sol.id = se.solicitante
   LEFT OUTER JOIN sgm_app.cat_predio cp ON cp.id = se.predio
   LEFT OUTER JOIN sgm_app.cat_predio_rustico cpr ON cpr.id = se.predio_rustico
   LEFT OUTER JOIN sgm_app.cat_parroquia cpq ON cpq.id = cpr.parroquia
   INNER JOIN sgm_financiero.ren_liquidacion l ON (l.anio = sa.anio) AND (l.predio = se.predio) OR (l.predio_rustico = se.predio_rustico)
WHERE
   sa.anio = $P{ANIO}
  -- AND L.estado_liquidacion = 2
   AND (se.predio = $P{ID_PREDIO}
   OR se.predio_rustico = $P{ID_PREDIO})]]>
	</queryString>
