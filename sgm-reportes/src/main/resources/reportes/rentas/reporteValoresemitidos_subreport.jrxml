	<parameter name="ANI" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select bl.descripcion, sum(dl.valor) as total_pagar,(select sum(dl.valor)from sgm_financiero.ren_liquidacion rl
inner join sgm_app.cat_predio cp on cp.id=rl.predio
inner join sgm_financiero.ren_det_liquidacion dl on dl.liquidacion=rl.id
inner join sgm_financiero.ren_rubros_liquidacion bl on bl.id=dl.rubro
where cp.estado='A' and rl.tipo_liquidacion=13 and rl.estado_liquidacion=2  and dl.rubro in(2,7,3)  AND RL.ANIO=$P{ANI} ) AS PRO_TOTAL
from sgm_financiero.ren_liquidacion rl
inner join sgm_app.cat_predio cp on cp.id=rl.predio
inner join sgm_financiero.ren_det_liquidacion dl on dl.liquidacion=rl.id
inner join sgm_financiero.ren_rubros_liquidacion bl on bl.id=dl.rubro
where cp.estado='A' and rl.tipo_liquidacion=13 and rl.estado_liquidacion=2 and dl.rubro in(2,7,3)  AND RL.ANIO=$P{ANI}
group by bl.descripcion]]>
	</queryString>
