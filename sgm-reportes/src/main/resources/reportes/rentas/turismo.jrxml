	<subDataset name="ACTIVIDAD" uuid="aa9600ae-0e9b-4aa2-99c6-676be02a6fc9">
		<parameter name="ID_LOCAL" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT
   a.descripcion
FROM
   sgm_financiero.ren_actividad_por_local actl
   INNER JOIN sgm_financiero.ren_actividad_comercial a ON a.id = actl.actividad
WHERE
   actl.local_comercial = $P{ID_LOCAL}]]>
		</queryString>
		<field name="descripcion" class="java.lang.String"/>
	</subDataset>
	<parameter name="LIQUIDACION" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="LOGO" class="java.lang.String">
		<defaultValueExpression><![CDATA["C:\\Users\\supergold\\Documents\\sgm\\src\\main\\webapp\\css\\homeIconsImages\\logo.jpg"]]></defaultValueExpression>
	</parameter>
	<parameter name="NUM_TRAMITE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    l.num_liquidacion AS NUM_LIQUIDACION,
    L.FECHA_INGRESO AS fecha_ingreso,
    L.OBSERVACION AS OBSERVACION,
    LU.DESCRIPCION AS UBICACION,
    LC.NUM_LOCAL AS NUM_LOCAL,
    LC.NOMBRE_LOCAL AS NOMBRE_LOCAL,
    LC.ID AS ID_LOCAL,
    L.TOTAL_PAGO AS TOTAL_CANCELAR,
    CASE WHEN E.ES_PERSONA THEN
       E.APELLIDOS || ' ' ||E.NOMBRES
    ELSE
       E.RAZON_SOCIAL END,
    E.CI_RUC AS RUC,
    CASE WHEN rz.ES_PERSONA THEN
       rz.APELLIDOS || ' ' ||rz.NOMBRES
    ELSE
       rz.RAZON_SOCIAL END AS razon_social_local,
    rz.CI_RUC AS RUC_razon_social,
    ct.descripcion AS categoria,
    ac.descripcion AS actividad,
    tt.valor AS valor_unitario,
    cca.cantidad AS cantidad,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM
     sgm_financiero.ren_liquidacion L LEFT OUTER JOIN sgm_financiero.ren_local_comercial LC ON LC.id = L.local_comercial
     LEFT OUTER JOIN sgm_financiero.ren_tipo_liquidacion TL ON L.tipo_liquidacion = TL.id
     LEFT OUTER JOIN sgm_app.cat_ente E ON E.id = L.comprador
     LEFT OUTER JOIN sgm_financiero.ren_local_ubicacion LU ON LC.ubicacion = LU.id
     LEFT OUTER JOIN sgm_app.cat_ente rz ON rz.id = LC.razon_social
     LEFT OUTER JOIN sgm_financiero.ren_tipo_local_comercial ct ON ct.id = lc.tipo_local
     INNER JOIN sgm_financiero.ren_actividad_por_local acl ON acl.local_comercial = lc.id
     LEFT OUTER JOIN sgm_financiero.ren_actividad_comercial ac ON ac.id = acl.actividad
     LEFT OUTER JOIN sgm_financiero.ren_local_categoria rlc ON rlc.descripcion = ct.descripcion
     LEFT OUTER JOIN sgm_financiero.ren_tasa_turismo tt ON (tt.actividad = acl.actividad AND tt.categoria = rlc.id)
     LEFT OUTER JOIN sgm_financiero.ren_local_cantidad_accesorios cca ON (/*cca.tipo_accesorio = 2 AND*/ cca.local_comercial = lc.id)
WHERE
     L.ID = $P{LIQUIDACION}
LIMIT 1]]>
	</queryString>
