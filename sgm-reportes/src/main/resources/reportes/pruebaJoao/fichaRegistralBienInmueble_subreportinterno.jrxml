	<parameter name="id_movimiento" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select calidad.papel, int.ced_ruc, int.nombre, c.nombre as domicilio, mov_cli.estado, int.tipo_interv

from sgm_app.reg_movimiento_cliente as mov_cli
left join sgm_app.reg_cat_papel as calidad on (calidad.id = mov_cli.papel)
left join sgm_app.reg_ente_interviniente int on (int.id = mov_cli.ente_interv)
left join sgm_app.cat_canton c on (mov_cli.domicilio=c.id)

where mov_cli.movimiento = $P{id_movimiento}]]>
	</queryString>
