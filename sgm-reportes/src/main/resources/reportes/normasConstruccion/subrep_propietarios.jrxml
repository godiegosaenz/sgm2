	<queryString>
		<![CDATA[SELECT
CASE  en2.es_persona
	when TRUE THEN
	coalesce(en2.apellidos,'')||' '||coalesce(en2.nombres,'')
	else
	en2.razon_social
	end AS apellido_responsable, en2.ci_ruc
 FROM sgm_app.cat_predio pr
 JOIN  sgm_app.cat_predio_propietario ppp ON pr.id=ppp.predio
 JOIN  sgm_app.cat_ente en2 ON ppp.ente=en2.id
 WHERE ppp.estado='A' AND pr.id=$P{id_predio}]]>
	</queryString>
	<field name="apellido_responsable" class="java.lang.String"/>
	<field name="ci_ruc" class="java.lang.String"/>
