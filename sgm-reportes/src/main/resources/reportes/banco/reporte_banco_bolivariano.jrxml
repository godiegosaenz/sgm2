	<queryString>
		<![CDATA[select b.num_predio, b.fecha_cobro,b.anio,t.clave_cat as codigo_nuevo, b.valor,b.estado,a.banco,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
from sgm_bancos.consolidacion_banco b
inner join sgm_app.cat_predio t on  t.num_predio=b.num_predio
inner join sgm_bancos.formato_banca  a on a.prefijo=b.banco
where b.banco='B-BOL'
ORDER BY b.num_predio,b.anio]]>
	</queryString>
