		<parameter name="ID_SOLICITUD" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT LIQ.FECHA_INGRESO, ID_LIQUIDACION, ANIO, EXO_LIQS.INTERES, EXO_LIQS.RECARGO
FROM SGM_FINANCIERO.FN_SOLICITUD_EXONERACIONES SOL
INNER JOIN SGM_APP.CAT_ENTE ENTE ON ENTE.ID = SOL.SOLICITANTE
INNER JOIN SGM_FINANCIERO.FN_EXONERACION_LIQUIDACIONES EXO_LIQS ON SOL.ID = EXO_LIQS.EXONERACION
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION LIQ ON EXO_LIQS.LIQUIDACION_NEW = LIQ.ID WHERE SOL.ID = $P{ID_SOLICITUD}]]>
		</queryString>
		<field name="fecha_ingreso" class="java.sql.Timestamp"/>
		<field name="id_liquidacion" class="java.lang.String"/>
		<field name="anio" class="java.lang.Integer"/>
		<field name="interes" class="java.math.BigDecimal"/>
		<field name="recargo" class="java.math.BigDecimal"/>
	</subDataset>
