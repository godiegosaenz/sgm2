	<queryString>
		<![CDATA[SELECT
CI_RUC,
COALESCE(RAZON_SOCIAL, '') || ''|| COALESCE(APELLIDOS, '') || ''|| COALESCE(NOMBRES, '') SOLICITANTE_ENTE,

(SELECT COALESCE(RAZON_SOCIAL, '') || ''|| COALESCE(APELLIDOS, '') || ''|| COALESCE(NOMBRES, '') FROM  )

INTERES, RECARGO, MULTAS, TOTAL_REMISION, TOTAL_PAGO, FECHA_PAGO_MAXIMO

FROM SGM_FINANCIERO.FN_SOLICITUD_EXONERACIONES SOL
INNER JOIN SGM_APP.CAT_ENTE ENTE ON ENTE.ID = SOL.SOLICITANTE WHERE SOL.ID = $P{ID_SOLICITUD}X			]]>
	</queryString>
