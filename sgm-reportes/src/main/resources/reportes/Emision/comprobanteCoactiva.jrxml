	<queryString>
		<![CDATA[SELECT LIQUIDACION.ID AS PK_LIQUIDACION, PAGO.FECHA_PAGO, PAGO.NUM_COMPROBANTE, PAGO.NUM_CAJERO, LIQUIDACION.ID_LIQUIDACION, TIPO.NOMBRE_TITULO, USUARIO.USUARIO,
       PRE.NUM_PREDIO, EN.CI_RUC, RUBRO.DESCRIPCION, CASE EN.ES_PERSONA WHEN TRUE THEN COALESCE(EN.NOMBRES,'')||' '||COALESCE(EN.APELLIDOS,'')
       ELSE EN.RAZON_SOCIAL END AS NOMBRES,LIQUIDACION.NOMBRE_COMPRADOR,
       CDLA.NOMBRE || ' MZ: ' || PRE.URB_MZ || ' SL: ' || PRE.URB_SOLARNEW AS DIRECCION,
       PREDIALANT, LIQUIDACION.OBSERVACION, PAGO.VALOR AS VALOR_PAGO , PAGO.INTERES, PAGO.DESCUENTO,PAGO.RECARGO
FROM SGM_FINANCIERO.REN_PAGO AS PAGO
LEFT JOIN SGM_FINANCIERO.REN_LIQUIDACION AS LIQUIDACION ON (LIQUIDACION.ID = PAGO.LIQUIDACION)
LEFT JOIN SGM_FINANCIERO.REN_TIPO_LIQUIDACION AS TIPO ON (LIQUIDACION.TIPO_LIQUIDACION = TIPO.ID)
LEFT JOIN SGM_FINANCIERO.REN_RUBROS_LIQUIDACION AS RUBRO ON (TIPO.ID = RUBRO.TIPO_LIQUIDACION AND RUBRO.CODIGO_RUBRO = 1)
LEFT JOIN SGM_APP.CAT_ENTE AS EN ON (EN.ID = PAGO.CONTRIBUYENTE)
LEFT JOIN SGM_APP.CAT_PREDIO AS PRE ON (PRE.ID = LIQUIDACION.PREDIO)
LEFT JOIN SGM_APP.CAT_CIUDADELA AS CDLA ON (CDLA.ID = PRE.CIUDADELA)
LEFT JOIN SGM_APP.ACL_USER AS USUARIO ON (USUARIO.ID = PAGO.CAJERO)
WHERE PAGO.ID = $P{ID_LIQUIDACION} AND LIQUIDACION.ESTADO_LIQUIDACION = 1 LIMIT 1]]>
	</queryString>
