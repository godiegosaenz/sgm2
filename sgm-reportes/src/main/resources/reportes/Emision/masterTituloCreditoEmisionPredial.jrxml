	<queryString>
		<![CDATA[SELECT TT.ID,

(

SELECT

SUM(		CASE WHEN (T.ANIO= EXTRACT(YEAR FROM NOW())) AND (EXTRACT(MONTH FROM NOW())<7) THEN

			(ROUND( D.VALOR*(
				SELECT PORCENTAJE FROM SGM_APP.CTLG_DESCUENTO_EMISION
				WHERE NUM_MES=EXTRACT(MONTH FROM NOW())
				AND NUM_QUINCENA=(CASE WHEN EXTRACT(DAY FROM NOW())>15 THEN 2 ELSE 1 END))/100,2)*(-1)) + T.SALDO

		WHEN (T.ANIO<EXTRACT(YEAR FROM NOW())) THEN
			((ROUND((D.VALOR*0.1),2) + ROUND( (T.SALDO) * (SELECT ROUND((PORCENTAJE / 100),2) FROM SGM_FINANCIERO.REN_INTERESES I WHERE I.ANIO = T.anio),2)))  + T.SALDO

		ELSE (ROUND((D.VALOR*0.1),2))  + T.SALDO

		END) AS VALOR_COMPLEMENTO

FROM SGM_FINANCIERO.REN_DET_LIQUIDACION D
INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION T ON D.LIQUIDACION = T.ID
WHERE $X{IN, T.ID, LIQUIDACIONES} AND D.RUBRO=2

) AS TOTAL_PAGO

 FROM SGM_FINANCIERO.REN_LIQUIDACION TT   WHERE $X{IN, TT.ID, LIQUIDACIONES}
ORDER BY  PREDIO, ANIO ASC]]>
	</queryString>
