	<queryString>
		<![CDATA[SELECT
     to_char(mej_obra.fecha_pago,'DD/MM/YYYY') AS mej_obra_fecha_pago,
   mej_obras.cuenta_contable AS mej_obras_cuenta_contable,
   mej_obras.concepto AS  mej_obras_concepto,
   SUM (mej_obra.valor) AS mej_obra_valor
FROM
mejoras.mej_det_rubro_mejoras mej_obra  inner join financiero.ren_det_liquidacion ren_det_liquidacion  ON mej_obra.rubro_mejora = ren_det_liquidacion.id
inner join mejoras.mej_valores_obra_ubicacion mej_valores ON mej_obra.ubicacion_obra = mej_valores.id
inner join mejoras.mej_obra mej_obras ON mej_obras.id = mej_valores.obra
 WHERE
  to_char(mej_obra.fecha_pago,'DD/MM/YYYY') BETWEEN $P{DESDE} AND $P{HASTA}
GROUP BY
     to_char(mej_obra.fecha_pago,'DD/MM/YYYY'),
     mej_obras.cuenta_contable,
     mej_obras.concepto,
     mej_obra.valor]]>
	</queryString>
