	<parameter name="ANIO" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[new java.lang.Integer(new java.text.SimpleDateFormat("yyyy").format(new java.util.Date()))]]></defaultValueExpression>
