	<parameter name="RUBROS" class="java.util.List"/>
	<queryString>
		<![CDATA[SELECT p.num_comprobante,p.fecha_pago::DATE,l.anio,
CASE WHEN l.comprador IS NULL THEN l.nombre_comprador
ELSE CASE en.es_persona WHEN true THEN COALESCE(en.apellidos,'')||' '||COALESCE(en.nombres,'') ELSE COALESCE(en.razon_social,'') end end as contribuyente,
pr.valor,
CASE WHEN l.predio IS NOT NULL THEN pru.sector||'-'||pru.mz||'-'||pru.cdla||'-'||pru.mzdiv||'-'||pru.solar||'-'||pru.div1||'-'||pru.div2||'-'||pru.div3||'-'||
pru.div4||'-'||pru.div5||'-'||pru.div6||'-'||pru.div7||'-'||pru.div8||'-'||pru.div9||'-'||pru.phv||'-'||pru.phh
ELSE CASE WHEN prr.id IS NOT NULL THEN prr.reg_catastral||'-'||prr.id_predial ELSE er.codigo_catastral END END as cod_predial
FROM sgm_financiero.ren_pago p
INNER JOIN sgm_financiero.ren_pago_rubro pr ON (p.id=pr.pago)
INNER JOIN sgm_financiero.ren_liquidacion l ON (p.liquidacion=l.id)
LEFT OUTER JOIN sgm_app.cat_ente en ON (l.comprador=en.id)
LEFT OUTER JOIN sgm_app.cat_predio_rustico prr ON (l.predio_rustico=prr.id)
LEFT OUTER JOIN sgm_app.emisiones_rurales_excel er ON (l.rural_excel=er.id)
LEFT OUTER JOIN sgm_app.cat_predio pru ON (l.predio=pru.id)
LEFT OUTER JOIN sgm_app.cat_parroquia prq ON (prr.parroquia=prq.id)
LEFT OUTER JOIN sgm_app.cat_ciudadela cdl ON (pru.ciudadela=cdl.id)
WHERE p.fecha_pago::DATE BETWEEN $P{DESDE} AND $P{HASTA}
AND $X{IN,pr.rubro,RUBROS}
AND p.estado
AND (CASE WHEN pr.rubro=21 THEN (CASE WHEN $P{TARIFA} THEN CASE WHEN prq.id IS NOT NULL THEN prq.id=3 ELSE er.parroquia='TARIFA' END ELSE CASE WHEN prq.id IS NOT NULL THEN prq.id=2 ELSE er.parroquia='SAMBORONDON' END END) ELSE TRUE END)
AND (CASE WHEN pr.rubro=12 THEN (CASE WHEN $P{TARIFA} THEN cdl.cod_parroquia=3 ELSE cdl.cod_parroquia!=3 END) ELSE TRUE END)
ORDER BY 2,1]]>
	</queryString>
