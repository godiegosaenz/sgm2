	<queryString>
		<![CDATA[SELECT
    rl.descripcion,
    p.fecha_pago,
    p.num_comprobante,
    p.estado,
    dl.desde,
    dl.hasta,
    dl.cantidad,
    dl.valor,
    u.usuario
FROM sgm_financiero.ren_pago  p
    INNER JOIN sgm_financiero.ren_det_liquidacion dl ON (p.liquidacion=dl.liquidacion)
    INNER JOIN sgm_financiero.ren_rubros_liquidacion rl ON (dl.rubro=rl.id)
    INNER JOIN sgm_app.acl_user u ON (p.cajero=u.id)
WHERE
     p.fecha_pago::DATE=$P{DESDE}
 AND dl.rubro IN (567, 568,569,570, 48)
ORDER BY
    rl.descripcion,
    dl.desde]]>
	</queryString>
