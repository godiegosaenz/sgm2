	<parameter name="USUARIO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select tipo.nombre_transaccion, pa.num_comprobante,
CASE WHEN liq.comprador IS NULL THEN liq.nombre_comprador ELSE
TRIM(UPPER(CASE en.es_persona WHEN TRUE THEN COALESCE(en.apellidos,'')||' '||COALESCE(en.nombres,'')
ELSE COALESCE(en.razon_social,'') END)) END contribuyente,
(pa.valor - pa.recargo - pa.interes + pa.descuento) as emision,
(CASE WHEN pa.estado THEN pa.descuento ELSE 0.00 END) AS descuento,
(CASE WHEN pa.estado THEN pa.interes ELSE 0.00 END) AS interes,
(CASE WHEN pa.estado THEN pa.recargo ELSE 0.00 END) AS recargo,
(CASE WHEN pa.estado THEN pa.valor ELSE 0.00 END) AS valor, pa.estado
from sgm_financiero.ren_pago as pa
left join sgm_financiero.ren_liquidacion as liq on (liq.id = pa.liquidacion)
left join sgm_financiero.ren_tipo_liquidacion as tipo on (tipo.id = liq.tipo_liquidacion)
left join sgm_app.cat_ente as en on (en.id = liq.comprador)
where pa.fecha_pago between to_timestamp($P{DESDE}, 'dd-mm-yyyy')
and to_timestamp($P{HASTA}, 'dd-mm-yyyy') and
(CASE WHEN $P{USUARIO} IS NOT NULL THEN pa.cajero = $P{USUARIO} ELSE TRUE END)
order by tipo.nombre_transaccion, pa.num_comprobante
/*
CASE WHEN ren_liquidacion.comprador IS NULL THEN ren_liquidacion.nombre_comprador ELSE TRIM(COALESCE(e.apellidos,'')||' '||COALESCE(e.nombres,'')||' '||COALESCE(e.razon_social,'')) END contribuyente,
*/]]>
