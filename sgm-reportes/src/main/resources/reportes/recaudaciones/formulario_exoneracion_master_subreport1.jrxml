	<queryString>
		<![CDATA[SELECT solicitud.num_resolucion_sac as num_resolucion,
fel.id as num_solicitud_sac, solicitud.fecha_ingreso,
en.ci_ruc,
CASE WHEN en.id IS NOT NULL THEN case en.es_persona when true then coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'') else en.razon_social end ELSE rl.nombre_comprador END as nombres,
en.direccion, en.fecha_nacimiento,
predio.sector ||'-'|| predio.mz ||'-'|| predio.cdla ||'-'|| predio.mzdiv ||'-'|| predio.solar
||'-'|| predio.div1 ||'-'|| predio.div2 ||'-'|| predio.div3 ||'-'|| predio.div4 ||'-'|| predio.div5
||'-'|| predio.div6 ||'-'|| predio.div7 ||'-'|| predio.div8 ||'-'|| predio.div9 ||'-'|| predio.phv
||'-'|| predio.phh as cod_nuevo, predio.predialant as cod_anterior, trunc(predio.avaluo_municipal, 2) as avaluo_municipal,
tipo.id as tipo, predio.num_predio,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa

FROM sgm_financiero.fn_exoneracion_liquidacion fel
INNER JOIN sgm_financiero.fn_solicitud_exoneracion solicitud
ON fel.exoneracion = solicitud.id
INNER JOIN sgm_financiero.ren_liquidacion rl
ON rl.id = fel.liquidacion
LEFT OUTER JOIN sgm_app.cat_ente en
ON en.id = rl.comprador
LEFT OUTER JOIN sgm_app.cat_predio predio
ON solicitud.predio = predio.id
INNER JOIN sgm_financiero.fn_exoneracion_tipo tipo
ON tipo.id = solicitud.exoneracion_tipo
WHERE fel.id = $P{ID_SOLICITUD}]]>
	</queryString>
