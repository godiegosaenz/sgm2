	<parameter name="HASTA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	liq.id_liquidacion titulo, lc.nombre_local, coalesce(ente.apellidos, ' ') ||
	coalesce(ente.nombres, ' ') ||  coalesce(ente.razon_social, ' ') propietario,
	lc.area, liq.total_pago, estado.descripcion estado_pago, sgm_app.get_mes_letra(liq.mes) || ' - '  || liq.anio AS emision
from sgm_financiero.ren_liquidacion liq
inner join sgm_financiero.ren_local_comercial lc ON lc.id = liq.local_comercial
inner join sgm_financiero.ren_estado_liquidacion estado ON estado.id = liq.estado_liquidacion
inner join sgm_app.cat_ente ente on ente.id = liq.comprador
where liq.tipo_liquidacion = 46
and  to_date(liq.mes || '-'  || liq.anio ,  'MM-YYYY')
between to_date($P{DESDE},  'MM-YYYY') AND to_date($P{HASTA},  'MM-YYYY')
order by lc.id asc]]>
	</queryString>
