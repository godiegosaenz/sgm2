	<queryString>
		<![CDATA[SELECT CASE WHEN l.anio=EXTRACT(YEAR FROM now()) THEN 1 ELSE 0 END AS tipo_mejora,
EXTRACT(YEAR FROM p.fecha_pago)::BIGINT AS anio_pago,
EXTRACT(MONTH FROM p.fecha_pago)::BIGINT AS mes,
SUM(pr.valor) AS valor_recaudado
FROM sgm_financiero.ren_pago p
INNER JOIN sgm_financiero.ren_pago_rubro pr ON (p.id=pr.pago)
INNER JOIN sgm_financiero.ren_liquidacion l ON (p.liquidacion=l.id)
WHERE l.tipo_liquidacion=13 AND p.estado=TRUE AND pr.rubro=7
AND p.fecha_pago::DATE BETWEEN $P{DESDE} AND $P{HASTA}
GROUP BY tipo_mejora, anio_pago, mes
ORDER BY tipo_mejora DESC, anio_pago ASC, mes ASC]]>
	</queryString>
