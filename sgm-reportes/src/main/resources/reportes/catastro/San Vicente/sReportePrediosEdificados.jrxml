	<queryString>
		<![CDATA[SELECT
	cp.clave_cat clave_catastral, cp.predialant clave_anterior, cp.num_predio matricula_inmobiliaria, trim(sgm_app.propietarios_por_predio(cp.id)) propietarios,
	barrio.nombre barrio,  (cp.calle || ' ' || cp.calle_s) calles,
	constructividad.valor constructividad
FROM sgm_app.cat_predio cp
LEFT JOIN sgm_app.cat_ciudadela barrio ON barrio.id = cp.ciudadela
INNER JOIN sgm_app.ctlg_item constructividad ON constructividad.id = cp.constructividad
WHERE cp.estado = 'A']]>
