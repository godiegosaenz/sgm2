	<queryString>
		<![CDATA[SELECT  COALESCE(ENTE.NOMBRES,'')||' '||COALESCE(ENTE.APELLIDOS,'') USUARIO,
pre.estado,
ciu.nombre as ciudadela,
ten.nombre as tenencia,
pre.tenencia_vivienda,
(SELECT COUNT(CP.ID) FROM SGM_APP.CAT_PREDIO CP
WHERE
pre.estado='A'
AND
TRUNC(CP.INST_CREACION)  >= $P{DESDE}
AND
TRUNC(CP.INST_CREACION) <= $P{HASTA}
AND
(((CP.USUARIO_CREADOR = $P{USUARIO}  ) OR ( CP.USUARIO_CREADOR IS NULL OR CP.USUARIO_CREADOR > $P{USUARIO} )))
)
TOTAL_USUARIO,
sgm_app.propietarios_por_predio(pre.id) propietario,
 clave_cat, predialant, parroquia, canton,  zona, sector, mz, solar, bloque, piso, unidad, tc.nombre as tipo_conjunto
 FROM  SGM_APP.CAT_PREDIO PRE

LEFT OUTER JOIN SGM_APP.ACL_USER US ON PRE.USUARIO_CREADOR = US.ID
LEFT OUTER JOIN SGM_APP.CAT_ENTE ENTE ON US.ENTE = ENTE.ID
LEFT OUTER JOIN sgm_app.cat_tipo_conjunto tc ON tc.id = pre.tipo_conjunto
LEFT OUTER JOIN sgm_app.cat_ciudadela ciu ON ciu.id = pre.ciudadela
LEFT OUTER JOIN sgm_app.ctlg_item item ON item.id = pre.tenencia_vivienda
LEFT OUTER JOIN sgm_app.cat_propiedad_item ten ON (pre.propiedad=ten.id)
WHERE
pre.estado='A'
AND
TRUNC(pre.INST_CREACION)  >= $P{DESDE}
AND
TRUNC(pre.INST_CREACION) <= $P{HASTA}
AND
(((USUARIO_CREADOR = $P{USUARIO}  ) OR ( USUARIO_CREADOR IS NULL OR USUARIO_CREADOR > $P{USUARIO} )))
ORDER BY pre.parroquia, pre.sector, pre.mz, pre.solar]]>
	</queryString>
