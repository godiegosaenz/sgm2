	<subDataset name="Linderos" whenResourceMissingType="Empty" uuid="2ad39ba9-6eeb-457f-8417-fa33b0eac8c6">
		<parameter name="idPredio" class="java.math.BigDecimal"/>
		<queryString>
			<![CDATA[SELECT i."VALOR" as orientacion,
       (LISTAGG(l."COLINDANTE", ', ') WITHIN GROUP (ORDER BY i."VALOR") ) as lindero,
       'Con' as con,
       (LISTAGG(l."PERIMETRO", ' + ') WITHIN GROUP (ORDER BY i."VALOR") ) as longitud ,
       SUM(l."PERIMETRO") as perimetro_total
  FROM "SGM_APP"."CAT_PREDIO_LINDEROS" l
  INNER JOIN "SGM_APP"."CTLG_ITEM" i ON l."ORIENTACION" = i."ID"
  INNER JOIN "SGM_APP"."CAT_PREDIO" p ON l."PREDIO" = p."ID"
  INNER JOIN "SGM_APP"."CAT_CERTIFICADO_AVALUO" c on c.PREDIO = p."ID"
  WHERE l."ESTADO" = 'A' AND l."PREDIO" =$P{idPredio}
  GROUP BY i."ID", i."VALOR"]]>
		</queryString>
		<field name="ORIENTACION" class="java.lang.String"/>
		<field name="LINDERO" class="java.lang.String"/>
		<field name="CON" class="java.lang.String"/>
		<field name="LONGITUD" class="java.lang.String"/>
		<field name="PERIMETRO_TOTAL" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="id" class="java.lang.Long" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Long(5999259)]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaReporte" class="java.lang.String"/>
	<parameter name="cantidadLetras" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT CA.ID,CA.FECHA,CA.SECUENCIA,CP.NUM_PREDIO,
CP.ID as id_predio,CP.SECTOR,
(CASE WHEN CP.CIUDADELA IS NULL THEN '' ELSE CIUDADELA.NOMBRE END) as nombreCiudaela, CP.MZ, CP.SOLAR, CP.BLOQUE, CP.PISO, CP.UNIDAD,
CA.CODIGO,
(CASE WHEN CE.ES_PERSONA = 1 THEN COALESCE(CE.APELLIDOS,'')||' '||COALESCE(CE.NOMBRES,'') ELSE CE.RAZON_SOCIAL END) AS SOLICITANTE, CA.DETALLE, CA.CODIGO_ACTUAL, CA.CODIGO_ANTERIOR, CA.AREA_SOLAR, CA.AVAL_SOLAR, CA.AVAL_CONSTRUCCION, CA.AVAL_CATASTRAL, CA.AVAL_PROPIEDAD, CA.USUARIO, CA.DIR_CAT, CA.POLIGONO, CA.ALICUOTA,
(SELECT descripcion from sgm_app.cat_provincia WHERE cod_nac = (select codigo_provincia from sgm_app.empresa)) as provincia,
(SELECT nombre from sgm_app.cat_canton WHERE codigo_nacional = (select codigo_canton from sgm_app.empresa)
AND id_provincia = (SELECT id from sgm_app.cat_provincia WHERE cod_nac = (select codigo_provincia from sgm_app.empresa))) as canton,
'ECUADOR' as pais
FROM sgm_app.CAT_CERTIFICADO_AVALUO CA
	INNER JOIN sgm_app.CAT_ENTE CE ON CE.ID = CA.SOLICITANTE
	INNER JOIN sgm_app.CAT_PREDIO CP ON CP.ID = CA.PREDIO
  LEFT JOIN CAT_CIUDADELA CIUDADELA ON CP.CIUDADELA = CIUDADELA.ID
WHERE CA.ID = $P{id}]]>
	</queryString>
