	<queryString>
		<![CDATA[SELECT DISTINCT DET.NO_EDIFICACION, CATEG.NOMBRE || ': '|| PROP.NOMBRE FACTOR, COALESCE(COEFICIENTES.VALOR_COEFICIENTE, 0.0000) COEFICIENTE,
DET.SUMA_COEFICIENTES_CONSTRUCCION SUMA
FROM SGM_APP.CAT_PREDIO_AVAL_DET_HIST DET
INNER JOIN SGM_APP.CAT_PREDIO_AVAL_HISTORICO CPAH ON CPAH.ID = DET.PREDIO_AVAL_HISTORICO
INNER JOIN SGM_APP.CAT_PREDIO_AVAL_DET_CONSTRUCT CONSTRU ON CONSTRU.ID_AVAL_DET_HIST = DET.ID
LEFT OUTER JOIN SGM_APP.CAT_EDF_PROP PROP ON CONSTRU.PROP =   PROP.ID
LEFT OUTER JOIN SGM_APP.CAT_EDF_CATEG_PROP CATEG ON PROP.CATEGORIA = CATEG.ID
LEFT OUTER JOIN SGM_APP.AVAL_COEFICIENTES_SUELO COEFICIENTES ON COEFICIENTES.CATEGORIA_CONSTRUCCION = PROP.ID
WHERE  CPAH.ID  =  $P{ID_PREDIO_HISTORICO}]]>
