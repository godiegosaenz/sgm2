	<queryString>
		<![CDATA[SELECT u.usuario,rl.descripcion, p.num_comprobante,p.fecha_pago,
CASE WHEN l.comprador IS NULL THEN l.nombre_comprador ELSE TRIM(COALESCE(e.apellidos,'')||' '||COALESCE(e.nombres,'')||' '||COALESCE(e.razon_social,'')) END contribuyente,
dl.desde,dl.hasta,dl.cantidad, dl.valor_recaudado,dl.valor,
l.nombre_comprador,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_financiero.ren_pago p
INNER JOIN sgm_financiero.ren_liquidacion l ON (l.id=p.liquidacion)
INNER JOIN sgm_financiero.ren_tipo_liquidacion tl ON (l.tipo_liquidacion=tl.id)
INNER JOIN sgm_financiero.ren_rubros_liquidacion rl ON (tl.id=rl.tipo_liquidacion)
INNER JOIN sgm_financiero.ren_det_liquidacion dl ON (p.liquidacion=dl.liquidacion AND rl.id=dl.rubro)
LEFT OUTER JOIN sgm_app.acl_user u ON (p.cajero=u.id)
LEFT OUTER JOIN sgm_app.cat_ente e ON (l.comprador=e.id)
WHERE p.estado = true AND
(p.fecha_pago::DATE BETWEEN $P{DESDE} AND $P{HASTA})
AND ((tl.codigo_titulo_reporte=192 AND rl.id IN (493231,493240 ,493242 ,493251 ,493261 ,493262 , 493264 , 493270 , 493272  ,493273 ,
493274 ,493275 ,493276 , 493277 ,493280 , 493282 , 493283 ,493290)) OR (tl.codigo_titulo_reporte=125 AND rl.id IN (48)))
ORDER BY u.usuario,rl.descripcion,p.num_comprobante]]>
	</queryString>
