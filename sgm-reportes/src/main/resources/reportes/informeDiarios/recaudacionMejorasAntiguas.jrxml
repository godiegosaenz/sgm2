	<queryString>
		<![CDATA[SELECT l.anio,SUM(pru.valor)
FROM sgm_financiero.ren_pago p
INNER JOIN sgm_financiero.ren_liquidacion  l ON (p.liquidacion=l.id)
INNER JOIN sgm_app.cat_predio pr ON (l.predio=pr.id)
INNER JOIN sgm_financiero.ren_pago_rubro pru ON (p.id=pru.pago)
WHERE p.fecha_pago::DATE BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
AND l.tipo_liquidacion =13 AND p.estado
AND pru.rubro=7 AND l.anio<2014
GROUP BY l.anio
ORDER BY l.anio DESC]]>
	</queryString>
