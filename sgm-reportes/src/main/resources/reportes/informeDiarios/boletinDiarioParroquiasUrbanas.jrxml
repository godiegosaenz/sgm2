	<parameter name="SUBREPORT_TITLE" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    L.ANIO,
    SUM(P.VALOR) AS TOTAL,
    (SELECT RAZON_SOCIAL FROM SGM_APPLICATION.EMPRESA) AS NOMBRE_EMPRESA
FROM
   SGM_FINANCIERO.REN_PAGO P
   INNER JOIN SGM_FINANCIERO.REN_LIQUIDACION L ON L.ID=P.LIQUIDACION
WHERE
   P.ESTADO AND P.FECHA_PAGO::DATE >= $P{DESDE}
GROUP BY 1]]>
	</queryString>
	<field name="anio" class="java.lang.Integer"/>
