	<queryString>
		<![CDATA[SELECT l.anio,o.cuenta_contable,o.concepto, SUM(prm.valor) as valor_recaudado
FROM sgm_financiero.ren_pago p
INNER JOIN sgm_financiero.ren_liquidacion  l ON (p.liquidacion=l.id)
INNER JOIN sgm_financiero.ren_pago_rubro pr ON (p.id=pr.pago)
INNER JOIN sgm_mejoras.mej_pago_rubro_mejora prm ON (pr.id=prm.rubro_mejora_pago)
INNER JOIN sgm_mejoras.mej_valores_obra_ubicacion  uo ON (prm.ubicacion_obra=uo.id)
INNER JOIN sgm_mejoras.mej_obra o ON (uo.obra=o.id)
WHERE p.fecha_pago::DATE BETWEEN $P{FECHA_DESDE} AND $P{FECHA_HASTA}
AND l.tipo_liquidacion =13 AND p.estado AND pr.rubro=7
GROUP BY l.anio,o.cuenta_contable,o.concepto
ORDER BY l.anio DESC,o.cuenta_contable]]>
	</queryString>
