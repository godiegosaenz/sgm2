	<queryString>
		<![CDATA[SELECT
     ren_rubros_liquidacion."descripcion" AS ren_rubros_liquidacion_descripcion,
     SUM (ren_det_liquidacion."valor") AS ren_det_liquidacion_valor
FROM
     sgm_financiero.ren_pago p INNER JOIN sgm_financiero.ren_pago_rubro ren_det_liquidacion ON p.id = ren_det_liquidacion.pago
     INNER JOIN sgm_financiero."ren_rubros_liquidacion" ren_rubros_liquidacion ON ren_rubros_liquidacion."id" = ren_det_liquidacion."rubro"
     INNER JOIN sgm_financiero."ren_tipo_liquidacion" ren_tipo_liquidacion ON ren_tipo_liquidacion."id" = ren_rubros_liquidacion."tipo_liquidacion"
WHERE
    ((ren_tipo_liquidacion.codigo_titulo_reporte=192 AND ren_rubros_liquidacion.id IN (567,568,569,570)) OR (ren_tipo_liquidacion.codigo_titulo_reporte=125 AND ren_rubros_liquidacion.id IN (48)))
    AND (p.fecha_pago::DATE BETWEEN $P{DESDE} AND $P{HASTA}) AND p.estado
group by
    ren_rubros_liquidacion.descripcion]]>
	</queryString>
