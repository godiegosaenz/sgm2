	<queryString>
		<![CDATA[SELECT  PR.CLAVE_CAT, LQ.ID_LIQUIDACION, EN.CI_RUC,
        CASE WHEN LQ.COMPRADOR IS NULL THEN LQ.NOMBRE_COMPRADOR ELSE
        UPPER(CASE EN.ES_PERSONA WHEN TRUE THEN
        TRIM(COALESCE(EN.APELLIDOS,'')||' '||COALESCE(EN.NOMBRES,''))ELSE TRIM(EN.RAZON_SOCIAL)END)END    AS CONTRIBUYENTE,
        LQ.TOTAL_PAGO , P.DESCUENTO, P.RECARGO, P.VALOR, LQ.ANIO,
        (SELECT NOMBRE_COMERCIAL FROM SGM_APPLICATION.EMPRESA) AS NOMBRE_MUNICIPIO,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
        FROM  SGM_FINANCIERO.REN_PAGO P
         INNER JOIN   SGM_FINANCIERO.REN_LIQUIDACION LQ ON LQ.ID = P.LIQUIDACION
         INNER JOIN  SGM_APP.CAT_PREDIO PR ON PR.ID = LQ.PREDIO
         LEFT OUTER  JOIN  SGM_APP.CAT_ENTE EN ON EN.ID = P.CONTRIBUYENTE
WHERE P.FECHA_PAGO::DATE = $P{DESDE};]]>
	</queryString>
