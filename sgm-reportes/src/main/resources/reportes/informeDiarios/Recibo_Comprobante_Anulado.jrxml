	<queryString>
		<![CDATA[SELECT
     ren_pago.num_comprobante AS ren_pago_num_comprobante,
     to_char(ren_pago.fecha_pago,'DD/MM/YYYY') AS ren_pago_fecha_pago,
     ren_pago.valor AS ren_pago_valor,
     CASE WHEN ren_liquidacion.comprador IS NULL THEN ren_liquidacion.nombre_comprador ELSE TRIM(COALESCE(e.apellidos,'')||' '||COALESCE(e.nombres,'')||' '||COALESCE(e.razon_social,'')) END n_contribuyente,
     acl_user.usuario AS acl_user_usuario,
     ren_tipo_liquidacion.nombre_titulo AS ren_tipo_liquidacion_nombre_titulo
FROM
     sgm_financiero."ren_tipo_liquidacion" ren_tipo_liquidacion RIGHT OUTER JOIN sgm_financiero."ren_liquidacion" ren_liquidacion ON ren_tipo_liquidacion."id" = ren_liquidacion."tipo_liquidacion"
LEFT OUTER JOIN sgm_financiero."ren_pago" ren_pago ON ren_pago.liquidacion = ren_liquidacion."id"
  LEFT OUTER JOIN sgm_app."cat_ente" e ON ren_liquidacion."comprador" = e."id"
  LEFT OUTER JOIN sgm_app.acl_user acl_user ON ren_pago.cajero = acl_user.id
 WHERE
      ren_pago.estado = false
 AND ren_pago.fecha_anulacion::DATE BETWEEN $P{DESDE} AND $P{HASTA}
GROUP BY
ren_tipo_liquidacion.nombre_titulo, ren_pago.num_comprobante, ren_pago.fecha_anulacion, ren_pago.fecha_pago,
ren_pago.valor, n_contribuyente, acl_user.usuario

ORDER BY ren_tipo_liquidacion.nombre_titulo,TO_DATE(ren_pago.fecha_anulacion::TEXT,'DD/MM/YYYY') asc]]>
	</queryString>
