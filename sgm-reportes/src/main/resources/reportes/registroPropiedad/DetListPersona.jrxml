	<parameter name="P_TRAMITE" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select ce.ci_ruc as cedruc, pa.papel, i.nombres as nombre_ingresado,
CASE ce.es_persona WHEN TRUE THEN ce.nombres||' '||ce.apellidos
ELSE ce.razon_social END nombres
from  sgm_flow.regp_liquidacion_derechos_aranceles a
left join sgm_flow.historico_tramites h on(h.id_tramite=a.historic_tramite)
left join sgm_flow.regp_intervinientes i on(i.liquidacion=a.id)
left join sgm_app.cat_ente ce on(i.ente=ce.id)
left join sgm_app.reg_cat_papel pa on(pa.id=i.papel)
where h.id=$P{P_TRAMITE}]]>
	</queryString>
