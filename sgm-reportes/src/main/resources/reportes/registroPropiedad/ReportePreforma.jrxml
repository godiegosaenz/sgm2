	<queryString>
		<![CDATA[select ac.nombre, det.cantidad as cantidad, det.valor_total as valor_pagar, liq.tasa_catastro, liq.is_exoneracion, liq.gastos_generales, liq.descuento_porc, liq.descuento_valor, ht.num_liquidacion,
liq.num_tramite_rp, ht.id as codigo, liq.id as liqder_id, liq.total_pagar as ttpagar, liq.cantidad_tasas_catastro, liq.fecha, ht.nombre_propietario as beneficiario, us.usuario,
en.ci_ruc as ced_beneficiario

from sgm_flow.historico_tramites ht
left join sgm_flow.regp_liquidacion_derechos_aranceles liq on (ht.id_tramite = liq.historic_tramite)
left join sgm_flow.regp_intervinientes cl on (cl.liquidacion = liq.id and cl.es_beneficiario = true)
left join sgm_app.cat_ente en on (en.id = cl.ente)
left join sgm_flow.regp_liquidacion_detalles det on(det.liquidacion = liq.id)
left join sgm_flow.regp_actos_ingreso ac on (ac.id = det.acto)
left join sgm_app.acl_user us on (us.id = liq.user_creador)
where ht.id=$P{P_TRAMITE} order by ac.tipo_acto asc]]>
	</queryString>
