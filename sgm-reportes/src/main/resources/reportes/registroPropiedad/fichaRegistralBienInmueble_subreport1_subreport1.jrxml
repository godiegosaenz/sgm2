	<parameter name="id_movimiento" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select
ref.secuencia,
mov.num_inscripcion, mov.fecha_inscripcion,
mov.folio_inicio, mov.folio_fin,
libro.nombre

from sgm_app.reg_movimiento_referencia as ref

left join sgm_app.reg_movimiento as mov on (mov.id = ref.movimiento_reff)
left join sgm_app.reg_libro as libro on (libro.id = mov.libro)

where ref.movimiento = $P{id_movimiento}

order by(ref.secuencia)]]>
	</queryString>
