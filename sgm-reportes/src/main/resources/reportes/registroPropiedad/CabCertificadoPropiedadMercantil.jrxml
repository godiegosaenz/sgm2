		<![CDATA[select m.id,to_char(m.fecha_inscripcion,'yyyy'),m.num_repertorio,m.num_tomo,m.folio_inicio,m.folio_fin,
m.num_inscripcion,a.nombre acto,l.nombre libro,a.abreviatura,m.num_paginas_contabilizada,
m.num_pagina_inscripcion,m.num_pagina_razon, m.es_negativa, m.observacion as anexo_negativa,
usuario.usuario, reg.nombre_completo, reg.titulo_completo,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa,
    (SELECT direccion  from sgm_application.empresa) AS empresa_direccion
from sgm_app.reg_movimiento m
left join sgm_app.reg_acto a on (m.acto=a.id)
left join sgm_app.reg_libro l on (m.libro=l.id)
left join sgm_app.acl_user usuario on(m.user_creador=usuario.id)
left join sgm_app.reg_registrador reg on(m.registrador=reg.id)
where m.id=$P{ID_MOV};]]>
