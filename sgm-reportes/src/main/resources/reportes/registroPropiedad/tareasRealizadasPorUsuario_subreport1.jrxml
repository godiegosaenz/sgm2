	<parameter name="USER_ID" class="java.lang.Long"/>
	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ht.num_tramite_x_departamento as tramite,
case en.es_persona when true then en.nombres || ' ' || en.apellidos else en.razon_social end as solicitante, ht.nombre_propietario as propietario, ce.observacion,
to_char(mo.fecha_inscripcion, 'dd-mm-yyyy') as fecha
from sgm_flow.regp_certificados_inscripciones as ce
inner join sgm_app.reg_movimiento as mo on (mo.regp_certificados_inscripciones = ce.id)
inner join sgm_flow.historico_tramites as ht on (ht.id = mo.num_tramite)
inner join sgm_app.cat_ente as en on (ht.solicitante = en.id)
where ce.tipo_tarea = 'I' and mo.estado = 'AC' and mo.user_creador = $P{USER_ID}
and mo.fecha_inscripcion between to_timestamp($P{DESDE}, 'dd-mm-yyyy')
and to_timestamp($P{HASTA}, 'dd-mm-yyyy') order by mo.fecha_inscripcion]]>
	</queryString>
