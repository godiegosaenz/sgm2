	<parameter name="IDPREDIO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select en.ci_ruc, en.direccion,

CASE en.es_persona when TRUE THEN
coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'')
else en.razon_social end AS nombres

from sgm_app.cat_predio_propietario as pp
left join sgm_app.cat_ente as en on (pp.ente = en.id)
where pp.estado = 'A' and pp.predio = $P{IDPREDIO}]]>
	</queryString>
