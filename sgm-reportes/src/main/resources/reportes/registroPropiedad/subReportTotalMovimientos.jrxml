	<parameter name="id_ficha" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select

libro.nombre,
count (libro.nombre)

from sgm_app.reg_movimiento_ficha as mov_ficha

left join sgm_app.reg_movimiento as mov on (mov.id = mov_ficha.movimiento)
left join sgm_app.reg_acto as acto on (acto.id = mov.acto)
left join sgm_app.reg_libro as libro on (libro.id = mov.libro)

where mov_ficha.ficha = $P{id_ficha}

group by libro.nombre
order by libro.nombre]]>
	</queryString>
	<field name="nombre" class="java.lang.String"/>
	<field name="count" class="java.lang.Long"/>
