	<queryString>
		<![CDATA[select t.id, t.fecha, t.nombre_propietario, t.urbanizacion, t.urbmz,
t.urbsolar, replace(t.telefonos,',',' -') telefono,correos, da.num_tramite_rp,
use.nombres||' '||use.apellidos as usuario, da.inf_adicional, da.fecha as fechaliq,
ens.ci_ruc AS cedula_solicitante, cl.nombres as beneficiario,

/*CASE en.es_persona when TRUE THEN
coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'')
else en.razon_social end AS beneficiario,*/

CASE ens.es_persona when TRUE THEN
coalesce(ens.nombres,'')||' '||coalesce(ens.apellidos,'')
else ens.razon_social end AS solicitante_nombre

from sgm_flow.historico_tramites t
left join sgm_flow.regp_liquidacion_derechos_aranceles da on (da.historic_tramite=t.id_tramite)
left join sgm_app.cat_ente ens on (ens.id=t.solicitante)
left join sgm_flow.regp_intervinientes cl on (cl.liquidacion=da.id and cl.es_beneficiario=true)
left join sgm_app.acl_user us on (us.id=da.user_creador)
left join sgm_app.cat_ente use on (use.id=us.ente)
where t.id=$P{p_idtramite}]]>
	</queryString>
