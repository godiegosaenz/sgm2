	<queryString>
		<![CDATA[SELECT
     ( SELECT ec."email" FROM sgm_app."ente_correo" ec WHERE ec."ente" = ce.id order by ec.id desc limit 1) AS mail,
     ( SELECT et."telefono" FROM sgm_app."ente_telefono" et WHERE et."ente" = et.id order by et.id desc limit 1) AS telefono,
     pdh."nombre_propietario" AS nombre_propietario,
     pdh."ci_ruc_prop" AS propietarios_predio_detalle_hist_ci_ruc_prop,
     ph."id" AS propietarios_predio_hist_id,
     ph."secuencia_escritura" AS propietarios_predio_hist_secuencia_escritura,
     ph."codigo_predial" AS propietarios_predio_hist_codigo_predial,
     ce."ci_ruc" AS ci_ruc,
     ce."nombres" AS nombres,
     ce."apellidos" AS apellidos,
     ce."es_persona" AS is_persona,
     ce."direccion" AS direccion,
     ce."nombre_comercial" AS cat_ente_nombre_comercial,
     ce."razon_social" AS razon_social
FROM
     sgm_app."propietarios_predio_hist" ph INNER JOIN sgm_app."propietarios_predio_detalle_hist" pdh ON ph."id" = pdh."propietarios_predio_detalle",
     sgm_app."cat_ente" ce
WHERE
     pdh."propietario_nuevo" = 0
 AND ph."predio" = $P{idpredio}
 AND ph."secuencia_escritura" = $P{secuencia}
 AND ce.id = pdh.ente]]>
	</queryString>
