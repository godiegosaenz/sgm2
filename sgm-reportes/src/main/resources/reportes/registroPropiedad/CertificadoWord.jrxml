	<queryString>
		<![CDATA[SELECT to_char(c.fecha_emision, 'yyyy') as fecha_emision,
c.OBSERVACION,CASE WHEN t.nombre_certificado = 'CERTIFICADO 014' THEN '' ELSE t.nombre_certificado END nombre_certificado, c.num_certificado, c.secuencia, c.num_tramite,
reg.nombre_completo, reg.titulo_completo
FROM sgm_app.reg_certificado c
left join sgm_app.reg_tipo_certificado t on(c.tipo_certificado= t.id)
left join sgm_app.reg_registrador reg on(c.registrador=reg.id)
where c.id=$P{ID_CERTIFICADO}]]>
	</queryString>
