	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[select lq.num_tramite_rp as tramite,
case en.es_persona when true then en.nombres || ' ' || en.apellidos else en.razon_social end as solicitante, ht.nombre_propietario as propietario, ce.observacion, to_char(ce.fecha_fin, 'dd-mm-yyyy') as fecha_fin
from sgm_flow.regp_certificados_inscripciones as ce
left join sgm_flow.regp_liquidacion_derechos_aranceles as lq on (lq.id = liquidacion)
left join sgm_flow.historico_tramites as ht on (ht.id_tramite = lq.historic_tramite)
left join sgm_app.cat_ente as en on (en.id = ht.solicitante)
where ce.tipo_tarea = 'CE' and ce.estado = true
and ce.fecha_fin between to_timestamp($P{DESDE}, 'dd-mm-yyyy')
and to_timestamp($P{HASTA}, 'dd-mm-yyyy') order by ce.fecha_fin]]>
	</queryString>
