	<subDataset name="dataset1" uuid="92b32890-ca25-451b-805e-259077c0050c">
		<parameter name="MOVIMIENTO" class="java.lang.Long"/>
		<parameter name="ENTE" class="java.lang.Long"/>
		<queryString>
			<![CDATA[select en.nombre as interviniente
from sgm_app.reg_movimiento_cliente mc
left join sgm_app.reg_ente_interviniente en on (en.id = mc.ente_interv)
where mc.movimiento = $P{MOVIMIENTO} and mc.ente_interv <> $P{ENTE}
order by en.nombre]]>
		</queryString>
		<field name="interviniente" class="java.lang.String"/>
	</subDataset>
	<parameter name="LIBRO" class="java.lang.Long"/>
	<parameter name="DESDE" class="java.util.Date"/>
	<parameter name="HASTA" class="java.util.Date"/>
	<parameter name="CADENA_HASTA" class="java.lang.String"/>
	<parameter name="USER_NAME" class="java.lang.String"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["C:\\Users\\Anyelo\\Desktop\\NetBeansProjects\\sgm\\src\\main\\webapp\\reportes\\registroPropiedad\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="LIBRO_NAME" class="java.lang.String"/>
	<queryString>
		<![CDATA[select re.id as ente, mo.id as movimiento,
re.nombre, ac.abreviatura as acto, pa.abreviatura as papel,
mo.num_inscripcion, mo.fecha_inscripcion, mo.num_repertorio,
mo.folio_inicio, mo.folio_fin, substring(li.nombre from 1 for 3)
as libro
from sgm_app.reg_movimiento_cliente mc
left join sgm_app.reg_ente_interviniente re on (re.id = mc.ente_interv)
left join sgm_app.reg_cat_papel pa on (pa.id = mc.papel)
left join sgm_app.reg_movimiento mo on (mo.id = mc.movimiento)
left join sgm_app.reg_acto ac on (ac.id = mo.acto)
left join sgm_app.reg_libro li on (li.id = mo.libro)
where mo.estado = 'AC' and
(CASE WHEN $P{LIBRO}>0 THEN mo.libro = $P{LIBRO} ELSE mo.libro > 0 END)
and mo.fecha_inscripcion between $P{DESDE} and $P{HASTA}
order by re.nombre, mo.fecha_inscripcion, mo.num_inscripcion,
mo.num_repertorio]]>
	</queryString>
