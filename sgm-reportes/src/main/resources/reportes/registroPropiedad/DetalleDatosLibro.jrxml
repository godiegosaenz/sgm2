	<parameter name="P_MOVIMIENTO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select lib.nombre, act.nombre as acto, rmo.num_inscripcion,rmo.fecha_inscripcion,rmo.folio_inicio,rmo.folio_fin
from sgm_app.reg_movimiento rm
left join sgm_app.reg_movimiento_referencia mr on(mr.movimiento = rm.id)
left join sgm_app.reg_movimiento rmo on(rmo.id = mr.movimiento_reff)
left join sgm_app.reg_libro lib on(lib.id = rmo.libro)
left join sgm_app.reg_acto act on(act.id = rmo.acto)
where rm.id=$P{P_MOVIMIENTO}]]>
	</queryString>
