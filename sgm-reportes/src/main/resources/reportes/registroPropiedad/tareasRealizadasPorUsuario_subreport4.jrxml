	<queryString>
		<![CDATA[select td.numero_solicitud, td.solicitante, td.institucion, ce.observacion,
to_char(ce.fecha_fin, 'dd-mm-yyyy') as fecha
from sgm_flow.regp_certificados_inscripciones as ce
left join sgm_flow.regp_tareas_dinardap as td on (td.id = ce.tarea_dinardap)
where (ce.tipo_tarea = 'DC' or ce.tipo_tarea = 'DW') and ce.estado = true
and ce.acl_user = $P{USER_ID}
and ce.fecha_fin between to_timestamp($P{DESDE}, 'dd-mm-yyyy')
and to_timestamp($P{HASTA}, 'dd-mm-yyyy') order by ce.fecha_fin]]>
	</queryString>
