	<parameter name="permiso" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select p.nombre, det.area, pe.altura, pe.pisos_snb, pe.pisos_bnb,cab.num_edificacion, cab.area_construccion,cab.desc_edificacion as descrip,
pe.cantidad_parqueos,pe.area_parqueos,pe.avaluo_liquidacion,cat.nombre as nombre_categoria, pe.area_solar as areasolar, pe.area_edificaciones as areaedif,
pe.desc_familiar as desc, pe.retiro_frontal as retf, pe.retiro_posterior as retp,
pe.retiro_lateral1 as retl, pe.retiro_lateral2 as retl2,pe.area_construccion as areaConstruccionPermiso,

CASE WHEN  cab.num_edificacion=0 THEN 'Edificacion Principal'
     WHEN  cab.num_edificacion=1 THEN 'Anexo 1'
     WHEN  cab.num_edificacion=2 THEN 'Anexo 2'
     WHEN  cab.num_edificacion=3 THEN 'Anexo 3'
     WHEN  cab.num_edificacion=4 THEN 'Anexo 4'
     WHEN  cab.num_edificacion=5 THEN 'Anexo 5'
     WHEN  cab.num_edificacion=6 THEN 'Anexo 6'
     WHEN  cab.num_edificacion=7 THEN 'Anexo 7'
     WHEN  cab.num_edificacion=8 THEN 'Anexo 8'
     WHEN  cab.num_edificacion=9 THEN 'Anexo 9'
     WHEN  cab.num_edificacion=10 THEN 'Anexo 10'
END as descEdificacion

from sgm_app.pe_permiso pe, sgm_app.pe_permiso_cab_edificacion cab
left outer join sgm_app.pe_detalle_permiso det on (cab.id=det.id_permiso_cab_edificacion)
left outer join sgm_app.cat_edf_prop p on (det.id_cat_edf_prop=p.id)
left outer join sgm_app.cat_edf_categ_prop cat on (p.categoria=cat.id)
where pe.id=$P{permiso}
and pe.id=cab.pe_permiso
ORDER BY cab.num_edificacion]]>
	</queryString>
