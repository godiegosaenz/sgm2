	<queryString>
		<![CDATA[select distinct(pe.id), p.nombre, det.area, pe.altura, cab.num_edificacion, cab.area_construccion,cab.desc_edificacion as descrip,
pe.cant_parqueos,pe.area_parqueos,pe.evaluo_liquidacion,pe.pisos_sbn,pe.pisos_bnb,cat.nombre as nombre_categoria, pe.area_solar as area_solar, pe.area_const as area_const,
pe.desc_edificacion as observacion, pe.retiro_frontal as retf,
pe.retiro_posterior as retp, pe.retiro_lateral1 as retl,
pe.retiro_lateral2 as retl2,

CASE WHEN  cab.num_edificacion=0 THEN 'Edificacion Principal'
     WHEN  cab.num_edificacion=1 THEN 'Anexo 1'
     WHEN  cab.num_edificacion=2 THEN 'Anexo 2'
     WHEN  cab.num_edificacion=3 THEN 'Anexo 3'
     WHEN  cab.num_edificacion=4 THEN 'Anexo 4'
     WHEN  cab.num_edificacion=5 THEN 'Anexo 5'
     WHEN  cab.num_edificacion=6 THEN 'Anexo 6'
     WHEN  cab.num_edificacion=7 THEN 'Anexo 7'
     WHEN  cab.num_edificacion=8 THEN 'Anexo 8'
     WHEN  cab.num_edificacion=9 THEN 'Anexo 9'
     WHEN  cab.num_edificacion=10 THEN 'Anexo 10'
END as descEdificacion



from sgm_app.pe_inspeccion_final pe
inner join sgm_app.pe_inspeccion_cab_edificacion cab on (pe.id = cab.id_inspeccion and cab.estado = true)
left outer join sgm_app.pe_detalle_inspeccion det on (cab.id=det.id_inspeccion_cab_edificacion and det.estado = true)
left outer join sgm_app.cat_edf_prop p on (det.id_cat_edf_prop=p.id)
left outer join sgm_app.cat_edf_categ_prop cat on (p.categoria=cat.id)
where pe.id=$P{inspeccion}
ORDER BY cab.num_edificacion]]>
	</queryString>
