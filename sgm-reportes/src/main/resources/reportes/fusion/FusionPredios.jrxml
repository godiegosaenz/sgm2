	<queryString>
		<![CDATA[SELECT HT.ID AS TRAMITE, HT.ID_PROCESO AS PROCESO,
	CASE E.ES_PERSONA WHEN TRUE THEN
		(E.NOMBRES || ' '||E.APELLIDOS)
		ELSE
		E.RAZON_SOCIAL
	END AS PROPIETARIO,
	E.CI_RUC AS CI_RUC,
	(SELECT NOM_COMPLETO FROM sgm_app.PE_FIRMA WHERE PROCESO=4 AND ID=2) AS DIRECTOR,
	(SELECT CARGO||' '||DEPARTAMENTO FROM sgm_app.PE_FIRMA WHERE PROCESO=4 AND ID=2) AS FIRMA,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_flow.HISTORICO_TRAMITES HT
	LEFT OUTER JOIN sgm_app.CAT_ENTE E ON HT.SOLICITANTE = E.ID
WHERE HT.ID = $P{id};]]>
	</queryString>
