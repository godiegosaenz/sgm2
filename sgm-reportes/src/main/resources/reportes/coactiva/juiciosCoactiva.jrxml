	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ju.numero_juicio, ju.fecha_juicio,
pre.clave_cat as cod_predial,
case en.es_persona when true then en.nombres || ' ' || en.apellidos else en.razon_social end as propietario, liq.anio, liq.total_pago,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
from sgm_financiero.coa_juicio as ju
left join sgm_financiero.coa_juicio_predio as jup on (ju.id = jup.juicio)
left join sgm_financiero.ren_liquidacion as liq on (liq.id = jup.liquidacion)
left join sgm_app.cat_predio as pre on (pre.id = liq.predio)
left join sgm_app.cat_ente as en on (en.id = liq.comprador)
where ju.fecha_ingreso between to_timestamp($P{FECHA_DESDE}, 'dd-mm-yyyy')
and to_timestamp($P{HASTA}, 'dd-mm-yyyy') order by ju.fecha_ingreso]]>
	</queryString>
