	<queryString>
		<![CDATA[SELECT pg.num_comprobante, pg.fecha_pago, l.anio,
CASE WHEN l.comprador IS NULL THEN l.nombre_comprador ELSE CASE e.es_persona WHEN true THEN e.nombres || ' ' || e.apellidos ELSE e.razon_social end end as contribuyente, cj.numero_juicio|| '-' ||
cj.anio_juicio num_juicio, pg.valor,
ca.detalle as abogado, p.clave_cat as cod_predial,
CASE WHEN pg.valor = l.total_pago + pg.recargo + pg.interes - pg.descuento THEN 'N' ELSE 'S' END AS abono
FROM sgm_financiero.ren_pago pg
INNER JOIN sgm_financiero.ren_liquidacion l ON pg.liquidacion = l.id
LEFT OUTER JOIN sgm_app.cat_ente e ON e.id = l.comprador
INNER JOIN sgm_financiero.coa_juicio_predio jp ON jp.liquidacion = l.id
INNER JOIN sgm_financiero.coa_juicio cj ON cj.id = jp.juicio
INNER JOIN sgm_app.cat_predio p ON p.id = jp.predio
INNER JOIN sgm_financiero.coa_abogado ca ON ca.id = jp.abogado_juicio
WHERE cj.fecha_ingreso::DATE BETWEEN $P{DESDE} AND $P{HASTA}
AND pg.estado AND jp.estado AND cj.estado
AND $X{IN,jp.abogado_juicio,ABOGADOS}
ORDER BY 7, 2 ASC]]>
	</queryString>
