	<queryString>
		<![CDATA[SELECT j.numero_juicio,j.anio_juicio,j.fecha_juicio,j.fecha_ingreso,j.total_deuda,h.id as tramite, e.descripcion as estado, p.num_predio,l.anio,  p.clave_cat as cod_predial,c.nombre as ciudadela,p.urb_solarnew,p.urb_mz,
    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa
FROM sgm_financiero.coa_juicio j
INNER JOIN sgm_financiero.coa_estado_juicio e ON (j.estado_juicio=e.id)
INNER JOIN sgm_financiero.coa_juicio_predio jp ON (j.id=jp.juicio)
INNER JOIN sgm_financiero.ren_liquidacion l ON (jp.liquidacion=l.id)
INNER JOIN sgm_app.cat_predio p ON (l.predio=p.id)
INNER JOIN sgm_app.cat_ciudadela c ON (p.ciudadela=c.id)
LEFT OUTER JOIN sgm_flow.historico_tramites h ON (j.tramite=h.id_tramite)
WHERE $X{IN,j.estado_juicio,ESTADOS}
AND (CASE WHEN $P{ANIO_DESDE} IS NOT NULL THEN j.anio_juicio>= $P{ANIO_DESDE} ELSE TRUE END)
AND (CASE WHEN $P{ANIO_HASTA} IS NOT NULL THEN j.anio_juicio<= $P{ANIO_HASTA} ELSE TRUE END)
AND (CASE WHEN $P{NUM_DESDE} IS NOT NULL THEN j.numero_juicio>= $P{NUM_DESDE} ELSE TRUE END)
AND (CASE WHEN $P{NUM_HASTA} IS NOT NULL THEN j.numero_juicio<= $P{NUM_HASTA} ELSE TRUE END)
AND (CASE WHEN $P{POR_JUICIO} THEN (j.fecha_juicio::DATE BETWEEN $P{F_JUICIO_DESDE} AND $P{F_JUICIO_HASTA}) ELSE TRUE END)
AND (CASE WHEN $P{POR_INGRESO} THEN (j.fecha_ingreso::DATE BETWEEN $P{F_INGRESO_DESDE} AND $P{F_INGRESO_HASTA}) ELSE TRUE END)
ORDER BY j.anio_juicio,j.numero_juicio,j.estado_juicio,j.fecha_juicio,j.fecha_ingreso,j.estado_juicio]]>
	</queryString>
