	<queryString>
		<![CDATA[select liq.num_liquidacion, liq.anio, liq.avaluo_municipal, liq.avaluo_construccion, liq.avaluo_solar, liq.fecha_ingreso, liq.total_pago,
CASE WHEN en IS NOT NULL THEN case en.es_persona when true then coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'') else en.razon_social end ELSE liq.nombre_comprador END as nombres,
CASE WHEN re IS NULL THEN pre.sitio ELSE re.sector END as direccion,
CASE WHEN re IS NULL THEN pre.reg_catastral || '-' || pre.id_predial || '-' || pq.descripcion ELSE re.codigo_catastral || ' - ' || re.parroquia END as cod_predial,
CASE WHEN re IS NULL THEN pre.id ELSE re.id END as num_predio
from sgm_financiero.ren_liquidacion as liq
left join sgm_app.cat_ente as en on (en.id = liq.comprador)
LEFT OUTER JOIN sgm_app.cat_predio_rustico as pre on (pre.id = liq.predio_rustico)
LEFT OUTER JOIN sgm_app.emisiones_rurales_excel re ON re.id=liq.rural_excel
LEFT OUTER JOIN sgm_app.cat_parroquia pq ON pq.id=pre.parroquia
where liq.id = $P{ID_TITULO}]]>
	</queryString>
