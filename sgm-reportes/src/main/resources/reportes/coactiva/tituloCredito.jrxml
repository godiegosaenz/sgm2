	<queryString>
		<![CDATA[SELECT LIQ.NUM_LIQUIDACION, LIQ.ANIO, LIQ.AVALUO_MUNICIPAL, LIQ.AVALUO_CONSTRUCCION, LIQ.AVALUO_SOLAR, LIQ.FECHA_INGRESO, LIQ.TOTAL_PAGO, PRE.NUM_PREDIO,
CASE WHEN LIQ.COMPRADOR IS NULL THEN LIQ.NOMBRE_COMPRADOR  ELSE CASE EN.ES_PERSONA WHEN TRUE THEN COALESCE
(EN.APELLIDOS,'')||' '||COALESCE(EN.NOMBRES,'') ELSE COALESCE(EN.RAZON_SOCIAL,'')  END END AS NOMBRES,
CDLA.NOMBRE || ' MZ: ' || PRE.URB_MZ || ' SL: ' || PRE.URB_SOLARNEW AS DIRECCION,
PRE.CLAVE_CAT AS COD_PREDIAL,
(SELECT RAZON_SOCIAL FROM SGM_APPLICATION.EMPRESA),

(

SELECT

	CASE WHEN (LIQ.ANIO= EXTRACT(YEAR FROM NOW())) AND (EXTRACT(MONTH FROM NOW())<7) THEN

			(ROUND( D.VALOR*(
				SELECT PORCENTAJE FROM SGM_APP.CTLG_DESCUENTO_EMISION
				WHERE NUM_MES=EXTRACT(MONTH FROM NOW())
				AND NUM_QUINCENA=(CASE WHEN EXTRACT(DAY FROM NOW())>15 THEN 2 ELSE 1 END))/100,2)*(-1))

		WHEN (LIQ.ANIO<EXTRACT(YEAR FROM NOW())) THEN
			((ROUND((D.VALOR*0.1),2) + ROUND( (LIQ.SALDO) * (SELECT ROUND((PORCENTAJE / 100),2) FROM SGM_FINANCIERO.REN_INTERESES I WHERE I.ANIO = liq.anio),2)))

		ELSE (ROUND((D.VALOR*0.1),2))

		END AS VALOR_COMPLEMENTO

FROM SGM_FINANCIERO.REN_DET_LIQUIDACION D
WHERE D.LIQUIDACION = LIQ.ID AND D.RUBRO=2

) AS VALOR_COMPLEMENTO,
liq.id_liquidacion

FROM SGM_FINANCIERO.REN_LIQUIDACION AS LIQ
LEFT JOIN SGM_APP.CAT_ENTE AS EN ON (EN.ID = LIQ.COMPRADOR)
LEFT JOIN SGM_APP.CAT_PREDIO AS PRE ON (PRE.ID = LIQ.PREDIO)
LEFT JOIN SGM_APP.CAT_CIUDADELA AS CDLA ON (CDLA.ID = PRE.CIUDADELA)
WHERE LIQ.ID = $P{ID_TITULO}]]>
	</queryString>
