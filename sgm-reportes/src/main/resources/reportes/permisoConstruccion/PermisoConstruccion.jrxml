	<queryString>
		<![CDATA[select

    (SELECT razon_social from sgm_application.empresa) AS nombre_empresa, pe.desc_familiar, pe.num_tram_ant,pe.nomb_urbanizacion_impresa, cdla.nombre ciudadela, tp.descripcion, pe.calle, pe.altura, pe.area_construccion, pe.tecnico_registro_profesional, en.reg_prof as registroprofesional, pe.area_implantacion,pe.area_solar, pe.linea_fabrica, (select (cargo||' '||departamento) from sgm_app.pe_firma where proceso=4 and id=2) as  firma, (select nom_completo from sgm_app.pe_firma where proceso=4 and id=2) as nom_completo, pe.retiro_frontal, pa.nombre plantaAlta, pb.nombre plantaBaja, pe.retiro_lateral1, pe.retiro_lateral2, pe.retiro_posterior, s.nombre estructura, t.nombre cubierta, u.nombre paredes, pe.observacion, pre.urb_mz, pre.urb_solarnew, i.nombre instalaciones, pre.sector|| '-' || pre.mz || '-' || pre.cdla || '-' || pre.mzdiv || '-' || COALESCE(pre.solar, 0) || '-' ||pre.div1 || '-' || pre.div2|| '-' ||  pre.div3  || '-' ||pre.div4 || '-' ||pre.div5|| '-' || pre.div6|| '-' ||  pre.div7  || '-' ||pre.div8 || '-' ||pre.div9 || '-' ||pre.phv || '-' ||pre.phh cod_catast_actual, pe.caracerrf, pe.caradosrf, pe.caracerrp, pe.caradosrp, pe.caracerrl1, pe.caradosrl1,pe.caracerrl2,pe.caradosrl2, extract(DAY from (pe.fecha_emision)) dia,extract(MONTH from pe.fecha_emision) mes, extract(YEAR from pe.fecha_emision) anio, extract(DAY from (pe.fecha_caducidad)) diac,extract(MONTH from pe.fecha_caducidad) mesc,extract(YEAR from pe.fecha_caducidad) anioc,
(SELECT ht.id FROM sgm_flow.historico_tramites  ht WHERE ht.id_tramite = pe.numero_tramite) ||'-'|| pe.anio_tramite tram, pe.urbmz, pe.urbsolar,
CASE  en.es_persona when TRUE THEN
	coalesce(en.titulo_prof,'')||' '||coalesce(en.apellidos,'')||' '||coalesce(en.nombres,'')
	else
	en.razon_social
	end AS apellido_responsable, en.ci_ruc AS cedula_responsable,

CASE  en2.es_persona when TRUE THEN
	coalesce(en2.apellidos,'')||' '||coalesce(en2.nombres,'')
	else
	en2.razon_social
	end AS apellido_propietario, en2.ci_ruc AS cedula_ruc_propietario

from   sgm_app.cat_predio pre left outer join sgm_app.cat_ciudadela cdla on cdla.id = pre.ciudadela,
sgm_app.pe_tipo_permiso tp, sgm_app.pe_permiso pe left outer join  sgm_app.cat_edf_prop s on  pe.prop_estructura=s.id
		   left outer join  sgm_app.cat_edf_prop t on  pe.prop_cubierta=t.id
		   left outer join  sgm_app.cat_edf_prop u on  pe.prop_paredes=u.id
		   left outer join  sgm_app.cat_edf_prop i on  pe.prop_instalaciones=i.id
		   left outer join  sgm_app.cat_edf_prop pa on pe.prop_plantaalta=pa.id
		   left outer join  sgm_app.cat_edf_prop pb on pe.prop_plantabaja=pb.id

		   left outer join  sgm_app.cat_ente en on pe.responsable_persona=en.id
		   left outer join  sgm_app.cat_ente en2 on pe.propietario_persona=en2.id

where  pe.id_predio=pre.id
and pe.tipo_permiso= tp.id
AND pe.id = $P{idpermiso}]]>
	</queryString>
