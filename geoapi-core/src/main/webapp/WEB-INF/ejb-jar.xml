<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        version="3.2"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">

    <enterprise-beans>
    <session>
      <ejb-name>CatastroGeoConfigs</ejb-name>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/wfsUrl</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>http://127.0.0.1:8081/geoserver/ows</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/wmsUrl</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>http://127.0.0.1:8081/geoserver/ows</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/wfsVersion</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>1.1.0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/wmsVersion</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>1.1.0</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/predioLayer</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>SanVicente:geo_solar</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/claveAttrName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>cod_predia</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/envelopeAdd</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>40.00</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/croquisLayer</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>SanVicente:croquis</env-entry-value>
      </env-entry>
      <env-entry>
        <env-entry-name>com.origami.geoapi.predios.CatastroGeoConfigs/srid</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>EPSG:32717</env-entry-value>
      </env-entry>
      
    </session>
  </enterprise-beans>
    
    
</ejb-jar>
